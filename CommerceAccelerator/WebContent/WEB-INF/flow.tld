<!DOCTYPE taglib 
	PUBLIC "-//Sun Microsystems, Inc.//DTD JSP Tag Library 1.2//EN"
			"http://java.sun.com/dtd/web-jsptaglibrary_1_2.dtd">

<taglib>
	<tlib-version>1.0</tlib-version>
	<jsp-version>1.2</jsp-version>
	<short-name>flexflow</short-name>
	<description>
		A tag library for dynamically inserting URLs into view JSPs at runtime.
	</description>

 	<!-- url tag -->
	<tag>
		<name>url</name>
		<tag-class>com.ibm.commerce.tools.devtools.flexflow.taglibs.URLTagHandler</tag-class>
		<body-content>empty</body-content>
		<attribute>
			<name>exitPort</name>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>condition</name>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>optimizedValue</name>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>routerJSP</name>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>urlType</name>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>userSpecifiedValue</name>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>

	<!-- ifDisabled tag -->
	<tag>
		<name>ifDisabled</name>
		<tag-class>com.ibm.commerce.tools.devtools.flexflow.taglibs.IfDisabledTagHandler</tag-class>
		<body-content>JSP</body-content>
		<attribute>
			<name>exitPort</name>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>feature</name>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>optimizedValue</name>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>userSpecifiedValue</name>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>

	<!-- ifEnabled tag -->
	<tag>
		<name>ifEnabled</name>
		<tag-class>com.ibm.commerce.tools.devtools.flexflow.taglibs.IfEnabledTagHandler</tag-class>
		<body-content>JSP</body-content>
		<attribute>
			<name>exitPort</name>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>feature</name>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>optimizedValue</name>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>userSpecifiedValue</name>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>

	<!-- fileRef tag -->
	<tag>
		<name>fileRef</name>
		<tag-class>com.ibm.commerce.tools.devtools.flexflow.taglibs.FileRefTagHandler</tag-class>
		<tei-class>com.ibm.commerce.tools.devtools.flexflow.taglibs.FileRefTEI</tei-class>
		<body-content>empty</body-content>
		<attribute>
			<name>fileId</name>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>id</name>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>optimizedValue</name>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>scope</name>
			<rtexprvalue>true</rtexprvalue>	
		</attribute>
		<attribute>
			<name>userSpecifiedValue</name>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
</taglib>
