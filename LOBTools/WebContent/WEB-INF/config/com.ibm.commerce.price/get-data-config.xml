<!-- Display Price -->
<data-type>
		<name>DisplayPrice</name>
		<type>com.ibm.commerce.price.facade.datatypes.DisplayPriceType</type>
</data-type>
<client-facade>
		<data-type-name>DisplayPrice</data-type-name>
