<Definitions>
	<!---
		This definition defines the properties of an attribute dictionary attribute.
	-->
	<ObjectProperties definitionName="cmc/catalog/AttributeDictionaryAttributePropertiesPage">
		<dependency localName="catalogResources" moduleName="cmc/catalog/CatalogResources"/>
		<PropertyTabs name="manageAttributeDictionaryAttributePropertyTab">
			<PropertyTabPane name="manageAttributeDictionaryAttributeProperties" text="${catalogResources.manageAttributeTab}">
				<PropertyPane baseDefinition="cmc/catalog/ManageAttributeDictionaryAttributePane"/>
			</PropertyTabPane>
			<PropertyTabPane name="manageAttributeDictionaryAttributeReferences" text="${catalogResources.attributeReferencesPrompt}">
				<PropertyPane name="attributeReferencesPane">
					<PropertyGroup collapsable="false" name="attributeFoldersGroup">
						<!-- Reference List: A table to display attribute folders that this attribute belongs to. -->
						<PropertyReferenceList listDefinition="cmc/catalog/AttributeFolderReferenceGrid" name="attributeFolderRefList" promptText="${catalogResources.attributeFolderReference}" referenceObjectPaths="*/AttributeFolderItemReference,*/AttributeFolderInheritedItemReference,*/InheritedAttributeFolderInheritedItemReference"/>
					</PropertyGroup>
					<PropertyGroup collapsable="false" name="attributeCatalogEntryReferencesGroup">
						<!-- Reference List: A table to display catalog entries that this attribute belongs to as a child list editor
							for local or asset store.
							This type of reference list enables paging and delete services.
						-->
						<EnablementAndCondition conditionId="eSiteStoreCondition">
							<EnablementCondition conditionId="b2cESiteStoreCondition" contextName="storeType" enablementValue="MHS" negate="true"/>
							<EnablementCondition conditionId="b2bESiteStoreCondition" contextName="storeType" enablementValue="BMH" negate="true"/>
						</EnablementAndCondition>
						<PropertyChildListEditor extendedHelpText="${catalogResources.extendedHelpText_AttributeCatalogEntryReference}" listDefinition="cmc/catalog/RefCatalogEntryGrid" name="CatalogEntryReferenceList" objectTypes="AttributeReferenceCatalogEntry" promptText="${catalogResources.attributeCatalogEntryReference}"/>
					</PropertyGroup>
					<PropertyGroup collapsable="false" name="inheritedAttributeCatalogEntryReferencesGroup">
						<!-- Reference List: A table to display catalog entries that this attribute belongs to as a child list editor.
							for an eSite store.
							This type of reference list enables paging and delete services.
						-->
						<EnablementOrCondition conditionId="eSiteStoreCondition">
							<EnablementCondition conditionId="b2cESiteStoreCondition" contextName="storeType" enablementValue="MHS"/>
							<EnablementCondition conditionId="b2bESiteStoreCondition" contextName="storeType" enablementValue="BMH"/>
						</EnablementOrCondition>
						<PropertyChildListEditor extendedHelpText="${catalogResources.extendedHelpText_InheritedAttributeCatalogEntryReference}" listDefinition="cmc/catalog/RefCatalogEntryGrid" name="InheritedCatalogEntryReferenceList" objectTypes="AttributeReferenceCatalogEntry" promptText="${catalogResources.attributeCatalogEntryReference}"/>
					</PropertyGroup>
				</PropertyPane>
			</PropertyTabPane>
		</PropertyTabs>
	</ObjectProperties>

	<!---
		This definition defines the property details displayed in the properties of an attribute dictionary attribute.
	-->
	<PropertyPane definitionName="cmc/catalog/ManageAttributeDictionaryAttributePane">
		<dependency localName="catalogResources" moduleName="cmc/catalog/CatalogResources"/>
		<PropertyGroup baseDefinition="cmc/foundation/PropertyAssetInformationGroup"/>
		<PropertyGroup groupTitle="${catalogResources.attributeProperties}" name="mainAttributeDictionaryAttributePropertyGroup" open="true">
			<!-- Attribute Unique ID -->
			<PropertyViewLongText promptText="${catalogResources.attributeDictionaryId}" propertyName="attrId">
				<FalseEnablementCondition/>
			</PropertyViewLongText>
			<PropertyInputText extendedHelpText="${catalogResources.extendedHelpText_Code}" promptText="${catalogResources.attributeCodePrompt}" propertyName="attributeCode" required="true"/>
			<PropertyInputText objectPath="AttributeDictionaryAttributeDescription" promptText="${catalogResources.attributeDisplayNamePrompt}" propertyName="attrName" required="true"/>
			<PropertyCombobox extendedHelpText="${catalogResources.extendedHelpText_AttributeDataType}" promptText="${catalogResources.attributeDataType_ColumnHeader}" propertyName="attrDataType" required="true"/>
			<PropertyInputMultiLineText objectPath="AttributeDictionaryAttributeDescription" promptText="${catalogResources.attributeDescriptionPrompt}" propertyName="attrDescription"/>
			<PropertyInputMultiLineText objectPath="AttributeDictionaryAttributeDescription" promptText="${catalogResources.attributeSecondaryDescriptionPrompt}" propertyName="xdescdata_SecondaryDescription"/>
			<PropertyCheckbox extendedHelpText="${catalogResources.extendedHelpText_displayable}" name="displayable" promptText="${catalogResources.attributeDisplayable}" propertyName="displayable"/>
			<PropertyCheckbox extendedHelpText="${catalogResources.extendedHelpText_comparable}" name="comparable" promptText="${catalogResources.attributeComparable}" propertyName="comparable"/>
			<PropertyGroup collapsable="false" name="storeDisplayGroup" open="true">
				<EnablementCondition conditionId="searchEnabledContextValueCondition" contextName="searchEnabled" enablementValue="true"/>
				<PropertyCheckbox extendedHelpText="${catalogResources.extendedHelpText_storeDisplay}" falseValue="0" helpLinkURL="tasks/tpnribbonad.htm" name="storeDisplay" promptText="${catalogResources.attributeStoreDisplay}" propertyName="storeDisplay" trueValue="1"/>
			</PropertyGroup>
			<PropertyGroup collapsable="false" name="merchandisableCheckBoxInformationGroup" open="true">
				<EnablementAndCondition conditionId="enableMerchandisableCondition">
					<EnablementCondition conditionId="searchEnabledContextValueCondition" contextName="searchEnabled" enablementValue="true"/>
					<EnablementOrCondition>
						<EnablementCondition checkObjectDefinition="true" conditionId="allowedValuesCondition" enablementValue="AttributeDictionaryAttributeWithAllowedValues" propertyName="objectType"/>
						<EnablementCondition checkObjectDefinition="true" conditionId="inheritedAllowedValuesCondition" enablementValue="InheritedAttributeDictionaryAttributeWithAllowedValues" propertyName="objectType"/>
					</EnablementOrCondition>
				</EnablementAndCondition>
				<PropertyCheckbox extendedHelpText="${catalogResources.extendedHelpText_merchandisable}" name="merchandisable" promptText="${catalogResources.attributeMerchandisable}" propertyName="merchandisable"/>
			</PropertyGroup>
			<PropertyGroup collapsable="false" name="facetableCheckBoxInformationGroup" open="true">
				<EnablementCondition conditionId="searchEnabledContextValueCondition" contextName="searchEnabled" enablementValue="true"/>
				<PropertyCheckbox extendedHelpText="${catalogResources.extendedHelpText_facetable}" name="facetable" promptText="${catalogResources.attributeFacetable}" propertyName="facetable"/>
			</PropertyGroup>
			<PropertyGroup collapsable="false" name="searchableCheckBoxInformationGroup" open="true">
				<EnablementAndCondition conditionId="facetableCondition1">
					<EnablementCondition conditionId="searchEnabledContextValueCondition" contextName="searchEnabled" enablementValue="true"/>
					<EnablementCondition conditionId="facetableTypeCondition" enablementValue="false" propertyName="facetable"/>
				</EnablementAndCondition>
				<PropertyCheckbox extendedHelpText="${catalogResources.extendedHelpText_searchable}" name="searchable" promptText="${catalogResources.attributeSearchable}" propertyName="searchable"/>
			</PropertyGroup>
			<PropertyInputText objectPath="AttributeDictionaryAttributeDescription" promptText="${catalogResources.attributeFootnotePrompt}" propertyName="xdescdata_Footnote"/>
			<PropertyCombobox objectPath="AttributeDictionaryAttributeDescription" promptText="${catalogResources.attributeValueUnitOfMeasure}" propertyName="xdescdata_UnitOfMeasure"/>
			<PropertyInputText extendedHelpText="${catalogResources.extendedHelpText_AttributeGroupName}" objectPath="AttributeDictionaryAttributeDescription" promptText="${catalogResources.attributeGroupNamePrompt}" propertyName="xdescdata_DisplayGroupName"/>
		</PropertyGroup>
		<!---
			This property group contains properties for a facetable attribute.
			This will only be displayed on the property group if the property 'facetable' is set to 'true'.
		-->
		<PropertyGroup groupTitle="${catalogResources.facetableProperties}" name="facetableInformationGroup" open="true">
			<!---
				An enablement condition that evaluates to true if 'facetable' is 'true' and the attribute has allowed values.
			-->
			<EnablementAndCondition conditionId="facetableCondition1">
				<EnablementCondition conditionId="searchEnabledContextValueCondition" contextName="searchEnabled" enablementValue="true"/>
				<EnablementCondition conditionId="facetableTypeCondition" enablementValue="true" propertyName="facetable"/>
			</EnablementAndCondition>
			<PropertyCheckbox extendedHelpText="${catalogResources.extendedHelpText_includeInKeywordSearch}" falseValue="false" name="xfacet_includeInKeywordSearch" promptText="${catalogResources.attributeFacetableIncludeInKeywordSearch}" propertyName="xfacet_includeInKeywordSearch" trueValue="true"/>
			<PropertyCheckbox extendedHelpText="${catalogResources.extendedHelpText_allowMultipleValueSelection}" falseValue="false" name="xfacet_allowMultipleValueSelection" promptText="${catalogResources.attributeFacetableAllowMultipleValueSelection}" propertyName="xfacet_allowMultipleValueSelection" trueValue="true"/>
			<PropertyCheckbox extendedHelpText="${catalogResources.extendedHelpText_displayZeroCountValues}" falseValue="false" name="xfacet_displayZeroCountValues" promptText="${catalogResources.attributeFacetableDisplayZeroCountValues}" propertyName="xfacet_displayZeroCountValues" trueValue="true"/>
			<PropertyRadioGroup extendedHelpText="${catalogResources.extendedHelpText_maximumValuesToDisplay}" name="xfacet_noMaximum" promptText="${catalogResources.attributeFacetableMaximumValuesToDisplay}" propertyName="xfacet_noMaximum"/>
			<PropertyGroup collapsable="false" open="true">
				<EnablementCondition conditionId="facetableNoMaxValueCondition" enablementValue="false" propertyName="xfacet_noMaximum"/>
				<PropertyStepper minimumValue="1" name="xfacet_maximumValuesToDisplay" propertyName="xfacet_maximumValuesToDisplay"/>
			</PropertyGroup>
			<PropertyRadioGroup extendedHelpText="${catalogResources.extendedHelpText_attributeFacetableValueOrder}" name="xfacet_valueOrder" promptText="${catalogResources.attributeFacetableValueOrder}" propertyName="xfacet_valueOrder"/>
			<PropertyInputText promptText="${catalogResources.field1Prompt}" propertyName="xfacet_facetField1" required="false"/>
			<PropertyInputText promptText="${catalogResources.field2Prompt}" propertyName="xfacet_facetField2" required="false"/>
			<PropertyInputText promptText="${catalogResources.field3Prompt}" propertyName="xfacet_facetField3" required="false"/>
		</PropertyGroup>
		<!---
			This property group contains the list of String/Text allowed values for a defining attribute.
			This will only be display on the property group if the property 'attrDataType' is set to 'String'.
		-->
		<PropertyGroup groupTitle="${catalogResources.attributeValuesGroupPrompt}" name="stringDefiningAttributePropGroup" open="true">
			<!---
				An enablement condition that evaluates to true if 'attrDataType' is 'String' and the attribute has allowed values.
			-->
			<EnablementAndCondition conditionId="stringDataTypeCondition1">
				<EnablementOrCondition>
					<EnablementCondition checkObjectDefinition="true" conditionId="allowedValuesCondition11" enablementValue="AttributeDictionaryAttributeWithAllowedValues" propertyName="objectType"/>
					<EnablementCondition checkObjectDefinition="true" conditionId="allowedValuesCondition12" enablementValue="InheritedAttributeDictionaryAttributeWithAllowedValues" propertyName="objectType"/>
				</EnablementOrCondition>
				<EnablementCondition conditionId="stringDataTypeCondition" enablementValue="String" propertyName="attrDataType"/>
			</EnablementAndCondition>
			<PropertyChildListEditor baseDefinition="cmc/catalog/AttributeDictionaryAttributeStringValueList"/>
		</PropertyGroup>
		<!---
			This property group contains the list of Integer/Whole Number allowed values for a defining attribute.
			This will only be display on the property group if the property 'attrDataType' is set to 'Integer'.
		-->
		<PropertyGroup groupTitle="${catalogResources.attributeValuesGroupPrompt}" name="intDefiningAttributePropGroup" open="true">
			<!---
				An enablement condition that evaluates to true if 'attrDataType' is 'Integer' and the attribute has allowed values.
			-->
			<EnablementAndCondition conditionId="integerDataTypeCondition1">
				<EnablementOrCondition>
					<EnablementCondition checkObjectDefinition="true" conditionId="allowedValuesCondition21" enablementValue="AttributeDictionaryAttributeWithAllowedValues" propertyName="objectType"/>
					<EnablementCondition checkObjectDefinition="true" conditionId="allowedValuesCondition22" enablementValue="InheritedAttributeDictionaryAttributeWithAllowedValues" propertyName="objectType"/>
				</EnablementOrCondition>
				<EnablementCondition conditionId="integerDataTypeCondition" enablementValue="Integer" propertyName="attrDataType"/>
			</EnablementAndCondition>
			<PropertyChildListEditor baseDefinition="cmc/catalog/AttributeDictionaryAttributeIntegerValueList"/>
		</PropertyGroup>
		<!---
			This property group contains the list of Float/Decimal Number allowed values for a defining attribute.
			This will only be display on the property group if the property 'attrDataType' is set to 'Float'.
		-->
		<PropertyGroup groupTitle="${catalogResources.attributeValuesGroupPrompt}" name="floatDefiningAttributePropGroup" open="true">
			<!---
				An enablement condition that evaluates to true if 'attrDataType' is 'Float' and the attribute has allowed values.
			-->
			<EnablementAndCondition conditionId="floatDataTypeCondition1">
				<EnablementOrCondition>
					<EnablementCondition checkObjectDefinition="true" conditionId="allowedValuesCondition31" enablementValue="AttributeDictionaryAttributeWithAllowedValues" propertyName="objectType"/>
					<EnablementCondition checkObjectDefinition="true" conditionId="allowedValuesCondition32" enablementValue="InheritedAttributeDictionaryAttributeWithAllowedValues" propertyName="objectType"/>
				</EnablementOrCondition>
				<EnablementCondition conditionId="floatDataTypeCondition" enablementValue="Float" propertyName="attrDataType"/>
			</EnablementAndCondition>
			<PropertyChildListEditor baseDefinition="cmc/catalog/AttributeDictionaryAttributeFloatValueList"/>
		</PropertyGroup>
		<PropertyGroup groupTitle="${catalogResources.attributeCustomProperties}" name="customAttributeDictionaryAttributePropertyGroup" open="false">
			<PropertyInputText extendedHelpText="${catalogResources.extendedHelpText_attributeDescriptionField1}" objectPath="AttributeDictionaryAttributeDescription" promptText="${catalogResources.attributeField1Prompt}" propertyName="xdescdata_Field1"/>
			<PropertyStepper extendedHelpText="${catalogResources.extendedHelpText_attributeField1}" maximumValue="2147483647" minimumValue="-2147483648" promptText="${catalogResources.attributePropertiesField1_DisplayName}" propertyName="xdata_Field1"/>
			<PropertyStepper extendedHelpText="${catalogResources.extendedHelpText_attributeField2}" maximumValue="2147483647" minimumValue="-2147483648" promptText="${catalogResources.attributePropertiesField2_DisplayName}" propertyName="xdata_Field2"/>
			<PropertyInputText extendedHelpText="${catalogResources.extendedHelpText_attributeField3}" promptText="${catalogResources.attributePropertiesField3_DisplayName}" propertyName="xdata_Field3"/>
		</PropertyGroup>
	</PropertyPane>

	<!---
		This definition is used to display the allowed values of a string attribute dictionary attribute.
	-->
	<PropertyChildListEditor definitionName="cmc/catalog/AttributeDictionaryAttributeStringValueList" helpLinkText="${foundationResources.helpDefaultLinkText}" helpLinkURL="tasks/tpnadatv.htm" listDefinition="cmc/catalog/AttributeDictionaryAttributeStringValueGrid" objectTypes="AttributeDictionaryAttributeStringValue,InheritedAttributeDictionaryAttributeStringValue" promptText="${catalogResources.attributeValuesPrompt}">
		<dependency localName="foundationResources" moduleName="cmc/foundation/FoundationResources"/>
		<dependency localName="catalogResources" moduleName="cmc/catalog/CatalogResources"/>
	</PropertyChildListEditor>

	<!---
		This definition is used to display the allowed values of a string attribute dictionary attribute.
	-->
	<ObjectGrid definitionName="cmc/catalog/AttributeDictionaryAttributeStringValueGrid" preferenceKey="catAttributeDictionaryAttributeStringValueGrid">
		<dependency localName="catalogResources" moduleName="cmc/catalog/CatalogResources"/>
		<GridNumeric editable="true" languageSensitive="true" name="displaySequence" numDecimalPlaces="1" objectPath="AttributeDictionaryAttributeValueDescription" propertyName="sequence" text="${catalogResources.displaySequence}" visible="true" width="220"/>
		<GridText editable="false" name="attrValId" propertyName="attrValId" text="${catalogResources.productUniqueId_ColumnHeader}" visible="false" width="150"/>
		<GridIconTypeImage name="typeIcon" propertyName="typeIcon" required="true" text="${catalogResources.catalogGroupType_ColumnHeader}" visible="true" width="60"/>
		<GridStoreName name="store"/>
		<GridText editable="false" name="xvaldata_AttributeValueIdentifier" propertyName="xvaldata_AttributeValueIdentifier" text="${catalogResources.attributeCodePrompt}" visible="false" width="250"/>
		<GridText datatype="string" editable="true" languageSensitive="true" name="value" objectPath="AttributeDictionaryAttributeValueDescription" propertyName="value" required="true" sortable="true" text="${catalogResources.attributeValueValue}" width="220"/>
		<GridCheckbox alignment="center" editable="true" enableFilter="true" falseValue="false" name="default" propertyName="default" text="${catalogResources.attributeValueUsage}" trueValue="true" visible="true" width="125"/>
		<!-- Extended fields for attribute allowed value -->
		<GridStepper datatype="number" editable="true" maximumValue="2147483647" minimumValue="-2147483648" name="xvaldata_AttributeValueField1" propertyName="xvaldata_AttributeValueField1" text="${catalogResources.attributeValue_Field1}" visible="false" width="250"/>
		<GridStepper datatype="number" editable="true" maximumValue="2147483647" minimumValue="-2147483648" name="xvaldata_AttributeValueField2" propertyName="xvaldata_AttributeValueField2" text="${catalogResources.attributeValue_Field2}" visible="false" width="250"/>
		<GridText editable="true" name="xvaldata_AttributeValueField3" propertyName="xvaldata_AttributeValueField3" text="${catalogResources.attributeValue_Field3}" visible="false" width="250"/>
		<!-- Language sensitive Extended fields for attribute allowed value -->
		<GridText editable="true" languageSensitive="true" name="xvaldescdata_Image1" objectPath="AttributeDictionaryAttributeValueDescription" propertyName="xvaldescdata_Image1" text="${catalogResources.attributeValueImage1}" visible="false" width="250"/>
		<GridText editable="true" languageSensitive="true" name="xvaldescdata_Image2" objectPath="AttributeDictionaryAttributeValueDescription" propertyName="xvaldescdata_Image2" text="${catalogResources.attributeValueImage2}" visible="false" width="250"/>
		<GridComboBox editable="true" enableFilter="true" languageSensitive="true" name="xvaldescdata_UnitOfMeasure" objectPath="AttributeDictionaryAttributeValueDescription" propertyName="xvaldescdata_UnitOfMeasure" text="${catalogResources.attributeValueUnitOfMeasure}" visible="false" width="200"/>
		<GridStepper datatype="number" editable="true" languageSensitive="true" maximumValue="2147483647" minimumValue="-2147483648" name="xvaldescdata_Field1" objectPath="AttributeDictionaryAttributeValueDescription" propertyName="xvaldescdata_Field1" text="${catalogResources.attributeValueDescription_Field1}" visible="false" width="250"/>
		<GridText editable="true" languageSensitive="true" name="xvaldescdata_Field2" objectPath="AttributeDictionaryAttributeValueDescription" propertyName="xvaldescdata_Field2" text="${catalogResources.attributeValueDescription_Field2}" visible="false" width="250"/>
		<GridText editable="true" languageSensitive="true" name="xvaldescdata_Field3" objectPath="AttributeDictionaryAttributeValueDescription" propertyName="xvaldescdata_Field3" text="${catalogResources.attributeValueDescription_Field3}" visible="false" width="250"/>
	</ObjectGrid>

	<!---
