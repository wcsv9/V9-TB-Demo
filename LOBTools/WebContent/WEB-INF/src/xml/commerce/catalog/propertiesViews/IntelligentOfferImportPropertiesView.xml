<Definitions>
	<ObjectProperties definitionName="cmc/catalog/IntelligentOfferImportProperties">
		<dependency localName="catalogResources" moduleName="cmc/catalog/CatalogResources"/>
		<PropertyPane name="pane">
			<PropertyGroup collapsable="false" name="group">
				<PropertyViewLongText name="filePath" promptText="${catalogResources.catalogImport_filePath}" propertyName="filePath"/>
				<PropertyViewLongText name="fileName" promptText="${catalogResources.catalogImport_fileName}" propertyName="fileName"/>
				<PropertyKilobyte name="fileSize" promptText="${catalogResources.catalogImport_fileSize}" propertyName="fileSize"/>
				<PropertyViewLongText name="status" promptText="${catalogResources.catalogImport_status}" propertyName="status" width="120"/>
				<PropertyViewLongText objectPath="IntelligentOfferImportSummary" promptText="${catalogResources.catalogImport_summary}" propertyName="summary"/>
			</PropertyGroup>
		</PropertyPane>
	</ObjectProperties>
</Definitions>