<Definitions>
	<!---
		This initialization service sets the context value 'lbsEnabled' corresponding to whether or not
		the location-based services feature is enabled.
	-->
	<InitService definitionName="cmc/foundation/EnabledFeatureLocationServicesInitService" url="/cmc/GetFeatureEnabled">
		<ServiceParam name="storeId"/>
		<ServiceParam parameterName="enabledFeatureName" value="location-services"/>
		<ServiceParam parameterName="enabledFeatureContextName" value="lbsEnabled"/>
	</InitService>
</Definitions>