<Definitions>
	<!---
		This definition displays the contents and characteristics of a static page
	-->
	<ObjectGrid definitionName="cmc/layout/StaticPageSearchGrid" preferenceKey="pglStaticPageSearchGrid">
		<dependency localName="pageLayoutResources" moduleName="cmc/layout/PageLayoutResources"/>
		<GridIconTypeImage name="typeIcon" propertyName="null" required="true" text="${pageLayoutResources.pageType_ColumnHeader}" visible="true" width="60"/>
		<GridText editable="false" name="pageType" propertyName="pageType" text="${pageLayoutResources.pageTypeName_ColumnHeader}" visible="false" width="90"/>
		<GridText editable="false" name="pageName" propertyName="name" text="${pageLayoutResources.pageName_ColumnHeader}" visible="true" width="90"/>
	</ObjectGrid>
</Definitions>