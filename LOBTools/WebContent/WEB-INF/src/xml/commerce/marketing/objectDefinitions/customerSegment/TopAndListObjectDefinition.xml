<Definitions>
	<!---
		<p>This is the top level andList for all customer segment condition. Every customer segment has this condition
		has its immediate child. All other customer segment conditions are children of this object definition.
		This definition defines the following properties:
		<ul>
		<li><code>elementName</code> - always has the value <code>topAndList</code></li>
		<li><code>conditionUsage</code> - always has the value <code>andListCondition</code></li>
		</ul>
		</p>
	-->
	<ChildObjectDefinition definitionName="cmc/marketing/TopAndListObjectDefinition" idProperty="elementName" objectType="TopAndList">
		<Xml name="template">
			<elementName>topAndList</elementName>
			<conditionUsage>andListCondition</conditionUsage>
		</Xml>
		<CreateService baseDefinition="cmc/marketing/CSElementCreateService"/>
		<UpdateService baseDefinition="cmc/marketing/CSElementUpdateService"/>
		<PropertyDefinition copyProtected="true" propertyName="conditionUniqueId" serverManaged="true"/>
		<ChildObjectDefinition baseDefinition="cmc/marketing/AcctAmountSpentObjectDefinition"/>
