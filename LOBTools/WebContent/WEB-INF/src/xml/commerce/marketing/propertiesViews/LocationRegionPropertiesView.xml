<Definitions>
	<ObjectProperties definitionName="cmc/marketing/LocationRegionProperties">
		<dependency localName="marketingResources" moduleName="cmc/marketing/MarketingResources"/>
		<PropertyPane name="pane">
			<PropertyGroup baseDefinition="cmc/foundation/PropertyAssetInformationGroup" name="assetGroup"/>
			<PropertyGroup groupTitle="${marketingResources.regionProperties}" hideHeaderWhenOnlyGroup="true" name="group" open="true">
				<PropertyViewLongText promptText="${marketingResources.regionId}" propertyName="regionId"/>
				<PropertyViewLongText promptText="${marketingResources.objectName}" propertyName="name"/>
				<PropertyViewLongText promptText="${marketingResources.description}" propertyName="description"/>
			</PropertyGroup>
		</PropertyPane>
	</ObjectProperties>
</Definitions>