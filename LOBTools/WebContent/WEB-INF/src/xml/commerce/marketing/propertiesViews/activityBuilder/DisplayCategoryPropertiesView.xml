<Definitions>
	<!---
		Properties view for the {@link cmc/marketing/DisplayCategoryElementObject Recommend Category action}.
	-->
	<FlowElementProperties definitionName="cmc/marketing/DisplayCategoryProperties">
		<dependency localName="marketingResources" moduleName="cmc/marketing/MarketingResources"/>
		<PropertyGroup collapsable="false" name="group">
			<PropertyChildListEditor listDefinition="cmc/marketing/CategorySequenceGrid" name="catEditor" objectTypes="ChildCatalogGroup,ChildInheritedCatalogGroup" promptText="${marketingResources.categories}" required="true" sequencePropertyName="sequence" validatorTypes="noCategoriesSelected"/>
		</PropertyGroup>
	</FlowElementProperties>
</Definitions>