<Definitions>
	<!---
		Properties view for the {@link cmc/marketing/DisplayContentElementObject Recommend Content action}.
	-->
	<FlowElementProperties definitionName="cmc/marketing/DisplayContentProperties">
		<dependency localName="marketingResources" moduleName="cmc/marketing/MarketingResources"/>
		<PropertyGroup collapsable="false" name="group">
			<PropertyChildListEditor listDefinition="cmc/marketing/ContentSequenceGrid" name="contentEditor" objectTypes="ChildMarketingContent,ChildInheritedMarketingContent" promptText="${marketingResources.content}" required="true" sequencePropertyName="sequence" validatorTypes="noMarketingContentSelected"/>
		</PropertyGroup>
	</FlowElementProperties>
</Definitions>