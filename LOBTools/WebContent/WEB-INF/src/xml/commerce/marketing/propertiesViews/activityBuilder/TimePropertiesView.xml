<Definitions>
	<!---
		Properties view for the {@link cmc/marketing/TimeElementObject Day and Time target}.
	-->
	<FlowElementProperties definitionName="cmc/marketing/TimeProperties">
		<dependency localName="marketingResources" moduleName="cmc/marketing/MarketingResources"/>
		<PropertyGroup collapsable="false" name="group">
			<PropertyDaysSelect helpText="${marketingResources.help_time_daysOfTheWeek}" promptText="${marketingResources.daysOfTheWeek}" propertyName="daysOfTheWeekString"/>
			<PropertyTimePicker helpText="${marketingResources.help_time_startTime}" promptText="${marketingResources.startTime}" propertyName="startTime"/>
			<PropertyTimePicker helpText="${marketingResources.help_time_endTime}" promptText="${marketingResources.endTime}" propertyName="endTime"/>
		</PropertyGroup>
	</FlowElementProperties>
</Definitions>