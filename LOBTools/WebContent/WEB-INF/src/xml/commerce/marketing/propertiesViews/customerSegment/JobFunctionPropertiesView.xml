	<PropertyGroup collapsable="false" definitionName="cmc/marketing/JobFunctionProperties" displayGrouping="true">
		<dependency localName="marketingResources" moduleName="cmc/marketing/MarketingResources"/>
		<PropertyCombobox objectPath="TopAndList/JobFunction" promptText="${marketingResources.cspJobFunction}" propertyName="template"/>
