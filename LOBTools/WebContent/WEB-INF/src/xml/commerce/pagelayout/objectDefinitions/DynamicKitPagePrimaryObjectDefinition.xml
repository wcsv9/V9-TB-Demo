<Definitions>
	<!--- This definition defines the base object from which DynamicKitBrowsingPage and InheritedDynamicKitBrowsingPage are derived.
	-->
	<PrimaryObjectDefinition baseDefinition="cmc/pagelayout/BaseCatalogEntryBrowsingPagePrimaryObjectDefinition" definitionName="cmc/pagelayout/BaseDynamicKitBrowsingPagePrimaryObjectDefinition" detailsViewHeaderText="${catalogResources.UtilityPaneHeaderText}" displayName="${pageLayoutResources.dynamickitPage_DisplayName}" helpLink="concepts/cpzpagecomptool.htm" isBaseDefinition="true">
		<dependency localName="pageLayoutResources" moduleName="cmc/pagelayout/PageLayoutResources"/>
		<dependency localName="catalogResources" moduleName="cmc/catalog/CatalogResources"/>
		<!--- This navigation list definition is used to display the layout assignments for this catalog entry page. -->
		<NavigationListDefinition displayName="${pageLayoutResources.navigation_layoutAssignmentListPrompt}" isDefault="true" listDefinition="cmc/pagelayout/PageLayoutReferenceList" listTitle="${pageLayoutResources.navigation_layoutAssignmentPrompt}" name="CatalogEntryBrowsingPageAssignmentChildList" toolbarIcon="listToolbarIcon"/>
		<!--- This navigation list definition is used to display the layout assignments for this catalog entry page as a calendar view. -->
		<NavigationListDefinition displayName="${pageLayoutResources.pageLayoutsGanttChartMenuTitle}" listDefinition="cmc/pagelayout/PageLayoutGanttReferenceList" listTitle="${pageLayoutResources.pageLayoutsGanttChartTitle}" name="CatalogEntryBrowsingPageAssignmentGanttChildList" toolbarIcon="ganttChartToolbarIcon"/>
	</PrimaryObjectDefinition>

	<!---
		This definition represents the primary object definition describing a Dynamic Kit Page as a business object.
	-->
	<PrimaryObjectDefinition baseDefinition="cmc/pagelayout/BaseDynamicKitBrowsingPagePrimaryObjectDefinition" definitionName="cmc/pagelayout/DynamicKitBrowsingPage" headerIcon="kitPageHeaderIcon" icon="kitPageIcon" objectType="DynamicKitBrowsingPage"/>

	<!---
		This definition represents the primary object definition describing an Inherited Dynamic Kit Page as a business object.
	-->
	<PrimaryObjectDefinition baseDefinition="cmc/pagelayout/BaseDynamicKitBrowsingPagePrimaryObjectDefinition" definitionName="cmc/pagelayout/InheritedDynamicKitBrowsingPage" displayName="${pageLayoutResources.inheritedDynamicKitPage_DisplayName}" headerIcon="inheritedKitPageHeaderIcon" icon="inheritedKitPageIcon" objectType="InheritedDynamicKitBrowsingPage">
		<dependency localName="pageLayoutResources" moduleName="cmc/pagelayout/PageLayoutResources"/>
	</PrimaryObjectDefinition>
</Definitions>