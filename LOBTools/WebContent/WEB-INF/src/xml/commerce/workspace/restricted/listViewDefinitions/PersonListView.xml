<Definitions>
	<!---
		@keywords private
	-->
	<ObjectGrid definitionName="cmc/workspace/ApproverGrid" preferenceKey="wspApproverGrid">
		<dependency localName="workspaceResources" moduleName="cmc/workspace/WorkspaceResources"/>
		<GridText editable="false" name="memberId" objectPath="Approver" propertyName="memberId" text="${workspaceResources.personMemberId}" visible="false" width="90"/>
		<GridText editable="false" name="distinguishedName" objectPath="Approver" propertyName="distinguishedName" text="${workspaceResources.personDistinguishedName}" visible="false" width="90"/>
		<GridText editable="false" name="logonId" objectPath="Approver" propertyName="logonId" required="true" text="${workspaceResources.personLogonId}" width="120"/>
		<GridText editable="false" name="firstName" objectPath="Approver" propertyName="firstName" text="${workspaceResources.personFirstName}" width="120"/>
		<GridText editable="false" name="lastName" objectPath="Approver" propertyName="lastName" text="${workspaceResources.personLastName}" width="120"/>
	</ObjectGrid>

	<ObjectGrid definitionName="cmc/workspace/ContributorGrid" preferenceKey="wspContributorGrid">
		<dependency localName="workspaceResources" moduleName="cmc/workspace/WorkspaceResources"/>
		<GridText editable="false" name="memberId" objectPath="Contributor" propertyName="memberId" text="${workspaceResources.personMemberId}" visible="false" width="90"/>
		<GridText editable="false" name="distinguishedName" objectPath="Contributor" propertyName="distinguishedName" text="${workspaceResources.personDistinguishedName}" visible="false" width="90"/>
		<GridText editable="false" name="logonId" objectPath="Contributor" propertyName="logonId" required="true" text="${workspaceResources.personLogonId}" width="120"/>
		<GridText editable="false" name="firstName" objectPath="Contributor" propertyName="firstName" text="${workspaceResources.personFirstName}" width="120"/>
		<GridText editable="false" name="lastName" objectPath="Contributor" propertyName="lastName" text="${workspaceResources.personLastName}" width="120"/>
	</ObjectGrid>

	<ObjectGrid definitionName="cmc/workspace/PersonSearchGrid" preferenceKey="wspPersonSearchGrid">
		<dependency localName="workspaceResources" moduleName="cmc/workspace/WorkspaceResources"/>
		<GridIconTypeImage name="typeIcon" propertyName="null" text="${workspaceResources.type}" width="100"/>
		<GridText editable="false" name="memberId" propertyName="memberId" text="${workspaceResources.personMemberId}" visible="false" width="90"/>
		<GridText editable="false" name="distinguishedName" propertyName="distinguishedName" text="${workspaceResources.personDistinguishedName}" visible="false" width="90"/>
		<GridText editable="false" name="logonId" propertyName="logonId" required="true" text="${workspaceResources.personLogonId}" width="120"/>
		<GridText editable="false" name="firstName" propertyName="firstName" text="${workspaceResources.personFirstName}" visible="false" width="120"/>
		<GridText editable="false" name="lastName" propertyName="lastName" text="${workspaceResources.personLastName}" visible="false" width="120"/>
	</ObjectGrid>
</Definitions>