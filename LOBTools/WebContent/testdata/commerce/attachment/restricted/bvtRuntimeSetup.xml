<testcase>
	
	<!-- Set up the data for Asset BVT -->
	<!-- Set a store value -->
	<action name="cmc/automation/AutoSetValueAction">
		<param name="valueKey" value="assetBVTStoreIdentifier"/>
		<param name="value" value="AuroraBVT"/>
	</action>
	
	<!-- Assign a value for attachment identifier -->
	<action name="cmc/automation/AutoSetValueAction">
		<param name="valueKey" value="bvt_attachment_identifier"/>
		<param name="value" value="bvt_new_attachment"/>
	</action>
	
	<!-- Assign a attachment code value -->
	<action name="cmc/automation/AutoSetValueAction">
		<param name="valueKey" value="bvt_attachment_code"/>
		<param name="value" value="bvt_new_attachment"/>
	</action>
	
	<!-- Assign a new directory name value for update-->
	<action name="cmc/automation/AutoSetValueAction">
		<param name="valueKey" value="bvt_directory_name"/>
		<param name="value" value="bvt_new_directory"/>
	</action>	
	
	<action name="cmc/automation/AutoSetValueAction">
		<param name="valueKey" value="bvt_updated_attachment_identifier"/>
		<param name="value" value="bvt_updated_attachment"/>
	</action>
	
	<!-- Assign an attachment code value -->
	<action name="cmc/automation/AutoSetValueAction">
		<param name="valueKey" value="bvt_updated_attachment_code"/>
		<param name="value" value="bvt_updated_attachment"/>
	</action>	
	
	

</testcase>