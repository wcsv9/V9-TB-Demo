<bean>
	<profiles>
		<profile name="IBM_Store_Details">
			<inputs>
				<input methodName="setCurrencyCode" inputName="currency" />
				<input methodName="setNumberUsage" inputName="numberUsage" />
			</inputs>
			<outputs>
				<output methodName="getCurrencyCode" outputName="currencyCode" />
				<output methodName="getCurrencySymbol" outputName="currencySymbol" />
				<output methodName="getDecimalPlaces" outputName="decimalPlaces" />
				<output methodName="getLangId" outputName="langId" />
				<output methodName="getNumberUsage" outputName="numberUsage" />
				<output methodName="getStoreId" outputName="storeId" />
			</outputs>
		</profile>
	</profiles>
</bean>
