<bean>
     <profiles>
          <profile name="IBM_RESOLVE_URLS">
               <inputs>
                    <input inputName="clickForwardUrl" methodName="setClickForwardUrl"/>
                    <input inputName="emailPromotionId" methodName="setEmailPromotionId"/>
                    <input inputName="optOutForwardUrl" methodName="setOptOutForwardUrl"/>
                    <input inputName="storeId" methodName="setStoreId"/>
                    <input inputName="urlHeader" methodName="setUrlHeader"/>
                    <input inputName="userId" methodName="setUserId"/>
               </inputs>
               <outputs>
                    <output methodName="getClickedEventUrl" outputName="clickedEventUrl"/>
                    <output methodName="getOpenedEventUrl" outputName="openedEventUrl"/>
                    <output methodName="getOptOutUrl" outputName="optOutUrl"/>
               </outputs>
          </profile>
     </profiles>
</bean>