<bean>
	<profiles>
		<profile name="IBM_Admin_Summary">
			<inputs>
				<input inputName="jobId" methodName="setJobId" />
			</inputs>
			<outputs>
				<output  outputName="resultList" methodName="getJobStatusDataBeans">
					<output outputName="actualStartTime" methodName="getActualStartInEntityType" />
					<output outputName="attemptsLeft" methodName="getAttemptsLeftInEntityType" />
					<output outputName="endTime" methodName="getEndInEntityType" />
					<output outputName="error" methodName="getError" />
					<output outputName="failedJobInstanceId" methodName="getRecoveryInstanceInEntityType" />
					<output outputName="jobId" methodName="getJobReferenceNumberInEntityType" />
					<output outputName="jobInstanceId" methodName="getInstanceReferenceNumberInEntityType" />
					<output outputName="lastUpdate" methodName="getLastUpdateInEntityType" />
					<output outputName="preferredStartTime" methodName="getPrefStartInEntityType" />
					<output outputName="progress" methodName="getProgress" />
					<output outputName="progressMessage" methodName="getProgressMessage" />
					<output outputName="queue" methodName="getQueue" />
					<output outputName="reasonCode" methodName="getReasonCode" />
					<output outputName="report" methodName="getReport" />
					<output outputName="result" methodName="getResult" />
					<output outputName="sequence" methodName="getSequenceInEntityType" />
					<output outputName="state" methodName="getState" />
				</output>
			</outputs>
		</profile>
	</profiles>
</bean>
