<_config:URLtoOAGIS
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.ibm.com/xmlns/prod/commerce/foundation/config ../xsd/url-to-oagis.xsd " xmlns:_config="http://www.ibm.com/xmlns/prod/commerce/foundation/config">

	<_config:URLDefinitions>	
	
		<_config:URLParameterGroup name="IBM_Admin_All" noun="Task" listName="resultList" flattened="false">
		
			<_config:URLParameter name="recordSetComplete" nounElement="/Show/@recordSetCompleteIndicator" key="false" return="true" preserveDataType="true" />
			<_config:URLParameter name="recordSetTotal" nounElement="/Show/@recordSetTotal" key="false" return="true" preserveDataType="true" />
			<_config:URLParameter name="recordSetCount" nounElement="/Show/@recordSetCount" key="false" return="true" preserveDataType="true" />
			<_config:URLParameter name="recordSetStartNumber" nounElement="/Show/@recordSetStartNumber" key="false" return="true" preserveDataType="true" />

			<_config:URLParameter name="usage" nounElement="/@usage" key="false" return="true" preserveDataType="true" />
			<_config:URLParameter name="status" nounElement="/@status" key="false" return="true" preserveDataType="true" />
			
			<_config:URLParameter name="taskId" nounElement="/TaskIdentifier/UniqueID" key="true" return="true" preserveDataType="true" />
			<_config:URLParameter name="taskExternalIdentifier" nounElement="/TaskIdentifier/ExternalIdentifier/Identifier" key="false" return="true" preserveDataType="true" />
			<_config:URLParameter name="taskName" nounElement="/TaskDescription/Name" key="false" return="true" preserveDataType="true" />
			<_config:URLParameter name="taskDescription" nounElement="/TaskDescription/Description" key="false" return="true" preserveDataType="true" />
			<_config:URLParameter name="parentTaskGroupID" nounElement="/TaskIdentifier/ExternalIdentifier/TaskGroupIdentifier/UniqueID" key="true" return="true" preserveDataType="true" />
			<_config:URLParameter name="parentTaskGroupExternalIdentifier" nounElement="/TaskIdentifier/ExternalIdentifier/TaskGroupIdentifier/ExternalIdentifier/Identifier" key="false" return="true" preserveDataType="true" />
			<_config:URLParameter name="parentTaskGroupName" nounElement="/TaskIdentifier/ExternalIdentifier/TaskGroupIdentifier/ExternalIdentifier/Name" key="false" return="true" preserveDataType="true" />
			<_config:URLParameter name="parentWorkspaceID" nounElement="/TaskIdentifier/ExternalIdentifier/TaskGroupIdentifier/ExternalIdentifier/ContainerIdentifier/UniqueID" key="false" return="true" preserveDataType="true" />
			<_config:URLParameter name="parentWorkspaceExternalIdentifier" nounElement="/TaskIdentifier/ExternalIdentifier/TaskGroupIdentifier/ExternalIdentifier/ContainerIdentifier/ExternalIdentifier/Identifier" key="false" return="true" preserveDataType="true" />
			<_config:URLParameter name="parentWorkspaceName" nounElement="/TaskIdentifier/ExternalIdentifier/TaskGroupIdentifier/ExternalIdentifier/ContainerIdentifier/ExternalIdentifier/Name" key="false" return="true" preserveDataType="true" />
			
			<_config:URLParameter name="TaskMembers/memberID" nounElement="/TaskMembers/TaskMember/Member/UniqueID" key="true" return="true" preserveDataType="true" />
			<_config:URLParameter name="TaskMembers/memberName" nounElement="/TaskMembers/TaskMember/Member/DistinguishedName" key="false" return="true" preserveDataType="true" />
			
			<_config:URLParameter name="dueDate" nounElement="/DueDate" key="false" return="true" preserveDataType="true" />
			<_config:URLParameter name="completionDate" nounElement="/CompletionDate" key="false" return="true" preserveDataType="true" />
			<_config:URLParameter name="commitDate" nounElement="/CommitDate" key="false" return="true" preserveDataType="true" />
			<_config:URLParameter name="promotionDate" nounElement="/PromotionDate" key="false" return="true" preserveDataType="true" />
			<_config:URLParameter name="approvalDate" nounElement="/ApprovalDate" key="false" return="true" preserveDataType="true" />
	
			<!-- UserData Section -->
			<_config:URLParameter name="x_" nounElement="/UserData/UserDataField" key="false" return="true" type="UserData" />
			<_config:URLParameter name="xdesc_" nounElement="/TaskDescription/UserData/UserDataField" key="false" return="true" type="UserData" />
			<_config:URLParameter name="xmembs_" nounElement="/TaskMembers/UserData/UserDataField" key="false" return="true" type="UserData" />
			<_config:URLParameter name="TaskMembers/xmemb_" nounElement="/TaskMembers/TaskMember/UserData/UserDataField" key="false" return="true" type="UserData" />

			
		</_config:URLParameterGroup>   

	</_config:URLDefinitions>

</_config:URLtoOAGIS>
