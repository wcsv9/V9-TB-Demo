<_config:Devices
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.ibm.com/xmlns/prod/commerce/foundation/config ../xsd/wc-devices.xsd" xmlns:_config="http://www.ibm.com/xmlns/prod/commerce/foundation/config">

	<!-- Declare Worklight hybrid app device prior to general phones and tablets -->
	<_config:DeviceGroup internalID="-44" channelID="-6">
		<_config:Device name="Worklight Android Tablet Hybrid" userAgentPattern=".*Worklight Android Tablet Hybrid.*"/>
	</_config:DeviceGroup>

	<_config:DeviceGroup internalID="-43" channelID="-6">
		<_config:Device name="Worklight iPhone Hybrid" userAgentPattern=".*Worklight iPhone Hybrid.*"/>
	</_config:DeviceGroup>

	<_config:DeviceGroup internalID="-42" channelID="-6">
		<_config:Device name="Worklight Android Hybrid" userAgentPattern=".*Worklight Android Hybrid.*"/>
	</_config:DeviceGroup>

	<_config:DeviceGroup internalID="-31" channelID="-6" >
		<_config:Device name="Apple iPad" userAgentPattern=".*iPad.*Safari.*" />
		<_config:Device name="BlackBerry Playbook" userAgentPattern=".*PlayBook.*Safari.*" />
		<_config:Device name="Android Tablets" userAgentPattern=".*Android.((?!Mobile).)*Safari.*" />
	</_config:DeviceGroup>

	<!-- Always declare Android hybrid device(-22) prior to Android general device(-21)  -->
	<_config:DeviceGroup internalID="-22" channelID="-6" >
		<_config:Device name="Android Hybrid Smartphone Client" userAgentPattern=".*Android Smartphone Hybrid.*" />
	</_config:DeviceGroup>

	<_config:DeviceGroup internalID="-21" channelID="-6" deviceClass="Mobile" >
		<_config:Device name="Apple iPhone" userAgentPattern=".*iPhone.*Mobile.*Safari.*" />
		<_config:Device name="Android Phones" userAgentPattern=".*Android.*Mobile.*Safari.*" />
		<_config:Device name="BlackBerry Phones" userAgentPattern=".*(BlackBerry|BB).*WebKit.*Mobile.*" />
		<_config:Device name="Windows Phones" userAgentPattern=".*Windows Phone.*IEMobile.*" />
	</_config:DeviceGroup>

	<_config:DeviceGroup internalID="-11" channelID="-6" >
		<_config:Device name="BlackBerry Bold 9000" userAgentPattern="BlackBerry9000.*" />
		<_config:Device name="BlackBerry Storm 9530" userAgentPattern="BlackBerry9500.*" />
		<_config:Device name="BlackBerry Curve 8320" userAgentPattern="BlackBerry8320.*"  />
		<_config:Device name="HTC S740" userAgentPattern=".*MSIEMobile.*" />
		<_config:Device name="Nokia S60" userAgentPattern=".*SymbianOS.*Series60/3.1.*Nokia3250.*" />
		<_config:Device name="Nokia N97" userAgentPattern=".*SymbianOS.*Series60/5.0.*Nokia3250.*" />
	</_config:DeviceGroup>

	<_config:DeviceGroup internalID="-12" channelID="-6" >
		<_config:Device name="Apple iPhone Hybrid App" userAgentPattern=".*iPhone.*" />
	</_config:DeviceGroup>

	<_config:DeviceGroup internalID="-32" channelID="-6" >
		<_config:Device name="Apple iPad Hybrid App" userAgentPattern=".*iPad.*" />
	</_config:DeviceGroup>

</_config:Devices>