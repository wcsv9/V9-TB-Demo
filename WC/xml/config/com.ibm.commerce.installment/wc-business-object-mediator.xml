<_config:BusinessObjectMediatorConfiguration
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.ibm.com/xmlns/prod/commerce/foundation/config ../xsd/wc-business-object-mediator.xsd"
	xmlns:_config="http://www.ibm.com/xmlns/prod/commerce/foundation/config">


	<!--
		The following is the read and change mediator configuration for the
		InstallmentRule noun.
	-->
	<!--
		TODO: Replace the physical type in the following line with the correct
		physical SDO generated by the DSL wizard.
	-->
	<_config:object
		logicalType="com.ibm.commerce.installment.facade.datatypes.InstallmentRuleType"
		physicalType="com.ibm.commerce.installment.facade.server.entity.datatypes.Insrule">


		<_config:mediator
			interfaceName="com.ibm.commerce.foundation.server.services.dataaccess.bom.mediator.ReadBusinessObjectMediator"
			className="com.ibm.commerce.installment.facade.server.services.dataaccess.bom.mediator.ReadInstallmentRuleMediator">
			<_config:part-mediator
				interfaceName="com.ibm.commerce.foundation.server.services.dataaccess.bom.mediator.ReadBusinessObjectPartMediator">
				<_config:part-mediator-implementation
					className="com.ibm.commerce.installment.facade.server.services.dataaccess.bom.mediator.ReadInstallmentRuleCatalogGroupAssociationMediator" />
				<_config:part-mediator-implementation
					className="com.ibm.commerce.installment.facade.server.services.dataaccess.bom.mediator.ReadInstallmentRuleCatalogEntryAssociationMediator" />
				<_config:part-mediator-implementation
					className="com.ibm.commerce.installment.facade.server.services.dataaccess.bom.mediator.ReadInstallmentRulePaymentMethodAssociationMediator" />
				<_config:part-mediator-implementation
					className="com.ibm.commerce.installment.facade.server.services.dataaccess.bom.mediator.ReadInstallmentRuleConditionMediator" />
				<_config:part-mediator-implementation
					className="com.ibm.commerce.installment.facade.server.services.dataaccess.bom.mediator.ReadInstallmentRuleInstallmentOptionMediator" />
			</_config:part-mediator>
		</_config:mediator>

		<_config:mediator
			interfaceName="com.ibm.commerce.foundation.server.services.dataaccess.bom.mediator.ChangeBusinessObjectMediator"
			className="com.ibm.commerce.installment.facade.server.services.dataaccess.bom.mediator.ChangeInstallmentRuleMediator"
			updateAccessProfile="IBM_Admin_Update">
			<_config:part-mediator
				interfaceName="com.ibm.commerce.foundation.server.services.dataaccess.bom.mediator.ChangeBusinessObjectPartMediator">
				<_config:part-mediator-implementation
					className="com.ibm.commerce.installment.facade.server.services.dataaccess.bom.mediator.ChangeInstallmentRuleBasePartMediator"
					updateAccessProfile="IBM_Admin_Update" />
				<_config:part-mediator-implementation
					className="com.ibm.commerce.installment.facade.server.services.dataaccess.bom.mediator.ChangeInstallmentRuleCatalogGroupAssociationPartMediator"
					updateAccessProfile="IBM_Admin_Update" />
				<_config:part-mediator-implementation
					className="com.ibm.commerce.installment.facade.server.services.dataaccess.bom.mediator.ChangeInstallmentRuleCatalogEntryAssociationPartMediator"
					updateAccessProfile="IBM_Admin_Update" />
				<_config:part-mediator-implementation
					className="com.ibm.commerce.installment.facade.server.services.dataaccess.bom.mediator.ChangeInstallmentRulePaymentMethodAssociationPartMediator"
					updateAccessProfile="IBM_Admin_Update" />
				<_config:part-mediator-implementation
					className="com.ibm.commerce.installment.facade.server.services.dataaccess.bom.mediator.ChangeInstallmentRuleConditionPartMediator"
					updateAccessProfile="IBM_Admin_Update" />
				<_config:part-mediator-implementation
					className="com.ibm.commerce.installment.facade.server.services.dataaccess.bom.mediator.ChangeInstallmentRuleInstallmentOptionPartMediator"
					updateAccessProfile="IBM_Admin_Update" />
			</_config:part-mediator>
		</_config:mediator>

		<_config:mediator
			className="com.ibm.commerce.context.content.locking.ChangeControlBusinessObjectMediatorImpl"
			interfaceName="com.ibm.commerce.foundation.server.services.dataaccess.bom.mediator.ChangeControlBusinessObjectMediator">
			<_config:mediator-properties>
				<_config:mediator-property name="uniqueIDProperty"
					value="InstallmentRuleIdentifier/UniqueID" />
				<_config:mediator-property name="resourceManagerIds"
					value="1" />
				<_config:mediator-property name="resourceContainerId"
					value="17" />
			</_config:mediator-properties>
		</_config:mediator> 
		
		<!-- INSTALLMENT CHANGE HISTORY MEDIATOR --> 
 
    <_config:mediator
			className="com.ibm.commerce.foundation.internal.server.services.changehistory.mediator.EchoBusinessObjectChangeHistoryMediatorImpl"
			interfaceName="com.ibm.commerce.foundation.server.services.dataaccess.bom.mediator.EchoBusinessObjectChangeHistoryMediator">
				<_config:mediator-properties> 
 
				  <_config:mediator-property name="nounName" 	value="InstallmentRule" />			  
				  <_config:mediator-property name="classNameForAdditionalService"
					value="com.ibm.commerce.installment.changehistory.InstallmentRuleChangeHistoryServiceImpl" />
			</_config:mediator-properties>
		</_config:mediator> 
		
		
		<!--
			Logical User Data to Physical data object mapping example.
			<_config:property-mapping
			logicalType="com.ibm.commerce.installment.facade.datatypes.InstallmentRuleType"
			physicalType="com.ibm.commerce.installment.facade.server.entity.datatypes.PhysicalSDOName">
			<_config:userDataProperty logicalPropertyName="LogicalUserDataName"
			physicalPropertyName="PhysicalUserDataName" />
			</_config:property-mapping>
		-->

	</_config:object>

</_config:BusinessObjectMediatorConfiguration>
