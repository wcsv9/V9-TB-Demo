<!-- ===================================================================================================
  This is the plug-in deployment descriptor for the WCPayments plug-in
==================================================================================================== -->
<Plugin xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../PluginProperties.xsd">
<!-- ===================================================================================================
  <MerchantConfInfo> includes the properties being passed to this plugin. It includes the following attributes:
  name:    The name of the plug-in property being passed to this plugin. 
  encrypt: If the value of the properity is encrypted.
==================================================================================================== -->
  <MerchantConfInfo name="userid" encrypt="false"/>
  <MerchantConfInfo name="password" encrypt="true"/>
  <MerchantConfInfo name="merchantNumber" encrypt="true"/>
</Plugin>
