<action resourceBundle="adminconsole.AdminConsoleNLS" 
        formName="userAdminForm"        
        helpKey="AC.access.usersGeneral.Help">

    <parameter
        listsize="12"
        startindex="0"
        endindex="0"
        resultsize="20"
        strSize="25" 
        charOnRight="5"        
        title="userAdminListTitle"
        ActionXMLFile="adminconsole.UserAdminList"
        refnum="0"
        orgId=""
        name=""
        roleId="" />

  <scrollcontrol
        title="users"
	display="true"/>
  

  <controlpanel
        display="true"/>
  
    
  <button>

	<menu name="find"
        	action="basefrm.findUserAdmin()" />
  	<menu name="newButton"
        	action="basefrm.newUserAdmin()" />
  	<menu name="changeButton"
        	action="basefrm.changeUserAdmin()"   
        	selection="single" />
  	<menu name="userGeneralRoles"
        	action="basefrm.rolesUserAdmin()"
        	selection="single" />
  	<menu name="userGeneralMbrGrp"
        	action="basefrm.mbrgrpUserAdmin()"
        	selection="single" />
  	<menu name="regCustCare"
        	action="basefrm.regCustCare()"
        	selection="single" />
 
  </button>
  
  <nlsNameFormats>

    <en_US>
	<name fields="lastName,firstName,middleName"/>
    </en_US>
    <fr_FR>
	<name fields="lastName,firstName,middleName"/>
    </fr_FR>
    <de_DE>
	<name fields="lastName,firstName,middleName"/>
    </de_DE>
    <it_IT>
	<name fields="lastName,firstName,middleName"/>
    </it_IT>
    <es_ES>
	<name fields="lastName,firstName,middleName"/>
    </es_ES>
    <pt_BR>
	<name fields="lastName,firstName,middleName"/>
    </pt_BR>
    <zh_CN>
	<name fields="lastName,firstName"/>
    </zh_CN>
    <zh_TW>
	<name fields="lastName,firstName"/>
    </zh_TW>
    <ko_KR>
	<name fields="lastName,firstName"/>
    </ko_KR>
    <ja_JP>
	<name fields="lastName,firstName"/>
    </ja_JP>
    <default>
	<name fields="lastName,firstName,middleName"/>
    </default>

  </nlsNameFormats>
  

</action>
