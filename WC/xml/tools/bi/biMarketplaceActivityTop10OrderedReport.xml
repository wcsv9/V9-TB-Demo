<Reporting>


<Report reportName="biMarketplaceActivityTop10OrderedReport" online="true" dataSourceName="WCA DataSource">
              <comment></comment>
              <SQLvalue>
select distinct
       P.PARTNUM AS "Part Number",
       coalesce(FOI.ECIM_FLAG, '-') AS "Configured",
       coalesce(FOI.UNIT_DESC, '-') AS "Description",
       FOI.NUM_UNITS AS "Num Units", 
	   FOI.UNIT_PRICE AS " Unit Price",
       FOI.ORDER_ID AS "Order Number Transferred",
       FOI.RPT_SALES_VALUE AS "Order Value Transferred",
       FOI.REPORT_CURRENCY  AS "Currency",
       coalesce(FOI.ECIM_FLAG, '-') AS "Configured",
       FOI.MEMBER_ID AS "Distributor User Id",
       SR.STORE_NAME AS "Distributor Organization"
FROM
       WCA.PRODUCT P,
       WCA.FACT_ORDERITEMS FOI,
       WCA.STORE_REF SR,
       WCA.PERIOD PER
WHERE
       (FOI.PER_ID=PER.PER_ID AND
       P.PRODUCT_ID=FOI.PRODUCT_ID AND
       FOI.STORE_ID IN ({storeent_id})) AND
       ((NOT FOI.DELETED_STATUS=1) 
       AND per.calendar_date &gt;= $DB_TIMESTAMP_FUNC({StartDate})$
       AND per.calendar_date &lt;= $DB_TIMESTAMP_FUNC({EndDate})$ 
       and SR.LANGUAGE_ID IN (
SELECT
       INTEGER(PR.PARAM_VALUE)
FROM
       WCA.PARAMETERS PR
WHERE
       PR.PARAM_TYPE='REPORT_LANGUAGE'))

ORDER BY
       {sortBy} {sortOrder}

              </SQLvalue>
