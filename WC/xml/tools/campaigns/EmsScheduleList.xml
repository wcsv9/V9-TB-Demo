<action resourceBundle="campaigns.campaignsRB"
	formName="emsForm"
	helpKey="MC.campaigns.EMarketingSpotDetail.Help">

	<parameter listsize="20"
		startindex="0"
		resultssize="0" />

	<scrollcontrol title="initiativeScheduleListTitle"
		display="false" />

	<controlpanel display="false" />

	<button>
		<menu name="new"
			action="basefrm.newInitiative()" />
		<menu name="properties"
			action="basefrm.changeInitiative()"
			selection="single" />
		<menu name="summary"
			action="basefrm.summaryInitiative()"
			selection="single" />
		<menu name="addnoellipsis"
			action="basefrm.addInitiative()" />
		<menu name="remove"
			action="basefrm.removeSchedule()"
			selection="multiple" />
