<dialog resourceBundle="campaigns.campaignsRB"
	windowTitle="productFindPanelTitle">

	<panel name="productFindPanel"
		url="CampaignProductFindPanelView"
		parameters="srPartNumber,srPartNumberType,srName,srNameType,srShortDescription,srShortDescriptionType"
		hasFinish="NO"
		hasCancel="NO"
		helpKey="MC.campaigns.ProductFind.Help" />

	<button name="find"
		action="findProducts()" />
	<button name="cancel"
		action="goBackToRefererURL()" />

	<jsFile src="/wcs/javascript/tools/campaigns/ProductFindDialog.js" />

