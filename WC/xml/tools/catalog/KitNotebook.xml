<notebook   resourceBundle="catalog.KitNLS"
            windowTitle="itemUpdateTitle"
            finishConfirmation="kitNotebookUpdateSuccess"
            cancelConfirmation="cancelConfirmation"
            finishURL="ProductNotebookUpdate" >
             
      
   <panel name="General"
         url="KitDetailView?"
         parameters="productrfnbr,langId"
         helpKey="MC.catalogTool.kitNotebookGeneralPanel.Help" />    
         
   <panel name="Description"
         url="ProductDescView?"
         parameters="productrfnbr,langId"
         helpKey="MC.catalogTool.kitNotebookDescriptionPanel.Help" />

   <panel name="Category"
         url="ProductCategoryView?"
         parameters="langId,productrfnbr"
         helpKey="MC.catalogTool.kitNotebookCategoryPanel.Help" />  
         
   <panel name="Image"
         url="ProductImageView?"
         parameters="productrfnbr,langId"
         helpKey="MC.catalogTool.kitNotebookImagesPanel.Help" />  
 
   <panel name="Manufacturer"
         url="ProductManufacturerView?"
         parameters="productrfnbr,langId"
         helpKey="MC.catalogTool.kitNotebookVendorPanel.Help" />
         
   <panel name="Discounts"
         url="ProductDiscountView?"
         parameters="productrfnbr,langId,storeId"
         helpKey="MC.catalogTool.kitNotebookDiscountsPanel.Help" />
    
   <panel name="SalesTax"
         url="ProductSalesTaxView?"
         parameters="productrfnbr,storeId,langId"
         helpKey="MC.catalogTool.kitNotebookSalesTaxPanel.Help" />
    
   <panel name="ShippingTax"
         url="ProductShippingTaxView?"
         parameters="productrfnbr,storeId,langId"
         helpKey="MC.catalogTool.kitNotebookShippingTaxPanel.Help" />
         
<panel name="UnitOfMeasure"
         url="ProductPackagingView?"
         parameters="langId,productrfnbr"
         helpKey="MC.catalogTool.kitNotebookPackagingPanel.Help" />

   <panel name="Fulfillment"
         url="ProductFulfillmentView?"
         parameters="langId,productrfnbr" 
	   	 hasTab ="NO"
         helpKey="MC.catalogTool.kitNotebookFulfillmentPanel.Help" />  

         
   <panel name="Advanced"
         url="ProductAdvancedView?"
         parameters="productrfnbr,langId"
	   	 hasTab ="YES"
         helpKey="MC.catalogTool.kitNotebookAdvancedPanel.Help" /> 
         
  <jsFile src="/wcs/javascript/tools/catalog/KitNotebook.js" />
  <jsFile src="/wcs/javascript/tools/common/Util.js" />
  <jsFile src="/wcs/javascript/tools/common/DateUtil.js" />
  
</notebook>
