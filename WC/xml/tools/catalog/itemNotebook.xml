<notebook   resourceBundle="catalog.ItemNLS"
            windowTitle="itemUpdateTitle"
            finishConfirmation="msgItemUpdateFinished"
            cancelConfirmation="cancelConfirmation"
            finishURL="ItemUpdate" >

   
   <panel name="General"
         url="ItemDetailView?"
         parameters="itemrfnbr,langId,productrfnbr"
         helpKey="MC.catalogTool.itemNotebookGeneralPanel.Help" />    
         
   <panel name="Description"
         url="ItemDescView?"
         parameters="itemrfnbr,langId,productrfnbr"
         helpKey="MC.catalogTool.itemNotebookDescriptionPanel.Help" />  
         
   <panel name="Image"
         url="ItemImageView?"
         parameters="itemrfnbr,langId,productrfnbr"
         helpKey="MC.catalogTool.itemNotebookImagesPanel.Help" />  

   <panel name="Attributes"
         url="ItemAttrView?"
         parameters="itemrfnbr,langId,productrfnbr"
         helpKey="MC.catalogTool.itemNotebookAttributesPanel.Help" />           
         
         
    <panel name="Manufacturer"
         url="ItemManufacturerView?"
         parameters="itemrfnbr,langId,productrfnbr"
         helpKey="MC.catalogTool.itemNotebookVendorPanel.Help" />
         
    <panel name="Discounts"
         url="ItemDiscountView?"
         parameters="itemrfnbr,langId,storeId,productrfnbr"
         helpKey="MC.catalogTool.itemNotebookDiscountsPanel.Help" />
    
    <panel name="SalesTax"
         url="ItemSalesTaxView?"
         parameters="itemrfnbr,storeId,langId,productrfnbr"
         helpKey="MC.catalogTool.itemNotebookSalesTaxPanel.Help" />
    
    <panel name="ShippingTax"
         url="ItemShippingTaxView?"
         parameters="itemrfnbr,storeId,langId,productrfnbr"
         helpKey="MC.catalogTool.itemNotebookShippingTaxPanel.Help" />
         
    <panel name="UnitOfMeasure"
         url="ItemPackagingView?"
         parameters="itemrfnbr,langId,productrfnbr" 
         helpKey="MC.catalogTool.itemNotebookPackagingPanel.Help" />
         
    <panel name="Advanced"
         url="ItemAdvancedView?"
         parameters="itemrfnbr,langId,productrfnbr"
         helpKey="MC.catalogTool.itemNotebookAdvancedPanel.Help" /> 
         
  <jsFile src="/wcs/javascript/tools/catalog/itemNotebookTest.js" />
  <jsFile src="/wcs/javascript/tools/common/Util.js" />
  <jsFile src="/wcs/javascript/tools/common/DateUtil.js" />
  
</notebook>
