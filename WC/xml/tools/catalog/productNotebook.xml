<notebook   resourceBundle="catalog.ProductNLS"
            windowTitle="itemUpdateTitle"
            finishConfirmation="productNotebookUpdateSuccess"
            cancelConfirmation="cancelConfirmation"
            finishURL="ProductNotebookUpdate" >
             
      
   <panel name="General"
         url="ProductDetailView?"
         parameters="productrfnbr,langId"
         helpKey="MC.catalogTool.productNotebookGeneralPanel.Help" />    
         
   <panel name="Description"
         url="ProductDescView?"
         parameters="productrfnbr,langId"
         helpKey="MC.catalogTool.productNotebookDescriptionPanel.Help" />

   <panel name="Category"
         url="ProductCategoryView?"
         parameters="langId,productrfnbr"
         helpKey="MC.catalogTool.productNotebookCategoryPanel.Help" />  
         
   <panel name="Image"
         url="ProductImageView?"
         parameters="productrfnbr,langId"
         helpKey="MC.catalogTool.productNotebookImagesPanel.Help" />  
 
   <panel name="Manufacturer"
         url="ProductManufacturerView?"
         parameters="productrfnbr,langId"
         helpKey="MC.catalogTool.productNotebookVendorPanel.Help" />
         
   <panel name="Discounts"
         url="ProductDiscountView?"
         parameters="productrfnbr,langId,storeId"
         helpKey="MC.catalogTool.productNotebookDiscountsPanel.Help" />
    
   <panel name="SalesTax"
         url="ProductSalesTaxView?"
         parameters="productrfnbr,storeId,langId"
         helpKey="MC.catalogTool.productNotebookSalesTaxPanel.Help" />
    
   <panel name="ShippingTax"
         url="ProductShippingTaxView?"
         parameters="productrfnbr,storeId,langId"
         helpKey="MC.catalogTool.productNotebookShippingTaxPanel.Help" />
         
<panel name="UnitOfMeasure"
         url="ProductPackagingView?"
         parameters="langId,productrfnbr"
         helpKey="MC.catalogTool.productNotebookPackagingPanel.Help" />

   <panel name="Fulfillment"
         url="ProductFulfillmentView?"
         parameters="langId,productrfnbr" 
         helpKey="MC.catalogTool.productNotebookFulfillmentPanel.Help" />

         
   <panel name="Advanced"
         url="ProductAdvancedView?"
         parameters="productrfnbr,langId"
         helpKey="MC.catalogTool.productNotebookAdvancedPanel.Help" /> 
         
  <jsFile src="/wcs/javascript/tools/catalog/productNotebook.js" />
  <jsFile src="/wcs/javascript/tools/common/Util.js" />
  <jsFile src="/wcs/javascript/tools/common/DateUtil.js" />
  
</notebook>
