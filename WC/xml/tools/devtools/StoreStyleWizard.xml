<wizard resourceBundle="devtools.StoreStyleRB"
	windowTitle="styleEditorTitle"
	finishURL="StoreStyleUpdate">

	<panel name="Style"
		url="StoreStyleWizardStylePanelView"
		hasCancel="YES"
		hasFinish="YES"
		hasBranch="NO"
		helpKey="MC.devtools.Style.Layout.Help" />
		
	<panel name="Colors"
		url="StoreStyleWizardColorsPanelView"
		hasCancel="YES"
		hasFinish="YES"
		hasBranch="NO"
		helpKey="MC.devtools.Style.Color.Help" />	

	<panel name="Banner"
		url="StoreStyleWizardSchemePanelView"
		hasCancel="YES"
		hasFinish="YES"
		hasBranch="NO"
		helpKey="MC.devtools.Style.Banner.Help" />	
	
	<button name="applyButton" action="applyButton()"/>
	<button name="viewStoreButton" action="viewStoreButton()"/>
  	
	<jsFile src="/wcs/javascript/tools/devtools/StyleEditor.js" />
	<jsFile src="/wcs/javascript/tools/common/Util.js" />
	<jsFile src="/wcs/javascript/tools/common/DateUtil.js" />	
</wizard>
