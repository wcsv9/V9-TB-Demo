<!-- This is the configuration file for the common objects that are
     available in the runtime model -->

<resourceConfig>

  <resource nameSpace="emailactivity">

    <!-- resource bundle file mappings -->
    <!-- THIS RESOURCE BUNDLE CONTAINS ALL NLV PROPERTIES FOR Email Activities -->
    <resourceBundle name="EmailActivityNLS"
                    bundle="com.ibm.commerce.tools.campaigns.properties.EmailActivityNLS" />

	<!-- This resource bundle contains properties for Coremetrics labels on email template editor 
	and some of the default Coremetrics parameters for link click/impression tracking -->
	<resourceBundle name="CMEmailActivity"
				bundle="com.ibm.commerce.tools.campaigns.properties.CMEmailActivity" />

    <!-- XML file mappings -->
    <resourceXML name="EmailActivityList"
        file="emailactivity/EmailActivityList.xml" />
    <resourceXML name="EmailActivityDialogAdd"
        file="emailactivity/EmailActivityDialogAdd.xml" />
    <resourceXML name="EmailActivityDialogChange"
        file="emailactivity/EmailActivityDialogChange.xml" />
    <resourceXML name="EmailActivitySummaryDialog"
        file="emailactivity/EmailActivitySummaryDialog.xml" />
	<resourceXML name="EmailTemplateList"
        file="emailactivity/EmailTemplateList.xml" />
    <resourceXML name="EmailTemplateDialog"
        file="emailactivity/EmailTemplateDialog.xml" />
    <resourceXML name="EmailTemplateDialogFromActivity"
        file="emailactivity/EmailTemplateDialogFromActivity.xml" />
    <resourceXML name="EMarketingSpotDialog"
        file="emailactivity/EMarketingSpotDialog.xml" />
    <resourceXML name="EMarketingSpotList"
        file="emailactivity/EMarketingSpotList.xml" />
    <resourceXML name="ContentSpotDialog"
        file="emailactivity/ContentSpotDialog.xml" />
    <resourceXML name="ContentSpotList"
        file="emailactivity/ContentSpotList.xml" />
    <resourceXML name="EmailTemplatePreviewDialog"
        file="emailactivity/EmailTemplatePreviewDialog.xml" />
  </resource>   

</resourceConfig>
