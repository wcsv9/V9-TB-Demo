<!-- This is the configuration file for the common objects that are
     available in the runtime model -->

<resourceConfig>

  <resource nameSpace="experiment">

    <!-- resource bundle file mappings -->
    <!-- THIS RESOURCE BUNDLE CONTAINS ALL NLV PROPERTIES FOR EXPERIMENTATION -->
    <resourceBundle name="experimentRB"
                    bundle="com.ibm.commerce.tools.experimentation.properties.ExperimentRB" />

    <!-- XML file mappings -->

    <resourceXML name="ExperimentList"
                 file="experimentation/ExperimentList.xml" />
    <resourceXML name="ExperimentNotebook"
                 file="experimentation/ExperimentNotebook.xml" />
    <resourceXML name="ExperimentGeneralDialog"
                 file="experimentation/ExperimentGeneralDialog.xml" />
    <resourceXML name="ExperimentContentSelectionDialog"
                 file="experimentation/ExperimentContentSelectionDialog.xml" />
    <resourceXML name="ExperimentSummaryDialog"
                 file="experimentation/ExperimentSummaryDialog.xml" />
    <resourceXML name="ExperimentDeleteDialog"
                 file="experimentation/ExperimentDeleteDialog.xml" />
    <resourceXML name="ExperimentStatusUpdateDialog"
                 file="experimentation/ExperimentStatusUpdateDialog.xml" />

    <!-- Mappings for e-Marketing Spot specific XML files -->

    <resourceXML name="ExperimentDefinitionEMarketingSpotDialog"
                 file="experimentation/ExperimentDefinitionEMarketingSpotDialog.xml" />
    <resourceXML name="ExperimentEMarketingSpotElementList"
                 file="experimentation/ExperimentEMarketingSpotElementList.xml" />
    <resourceXML name="ExperimentStatisticsEMarketingSpotList"
                 file="experimentation/ExperimentStatisticsEMarketingSpotList.xml" />
    <resourceXML name="ExperimentStatisticsEMarketingSpotNoButtonList"
                 file="experimentation/ExperimentStatisticsEMarketingSpotNoButtonList.xml" />

  </resource>

