<action resourceBundle="guidedSell.guidedSellRB"
	formName="guidedSellForm"
	helpKey="MC.guidedsell.gsmain.Help">

	<parameter listsize="20"
		startindex="0"
		resultssize="0"
		orderby="name" />

	<scrollcontrol title="guidedSellListTitle"
		display="true" />

	<button>
		<menu name="newGuidedSellButton"
		      action="basefrm.newGuidedSell()"/>

		<menu		name="changeGuidedSellButton"
				action="basefrm.changeGuidedSell()"
				selection="single" />

		<menu		name="searchCategoriesButton"
				action="basefrm.searchCategories()"/>

		<menu		name="qnaTreeButton"
				action="basefrm.qnaTree()"
				selection="single" />

		<menu		name="publishButton"
				action="basefrm.publish()"
				selection="single" />

		<menu		name="unpublishButton"
				action="basefrm.unpublish()"
				selection="single" />

		<menu		name="deleteGuidedSellButton"
				action="basefrm.deleteGuidedSell()"
				selection="multiple" />
	</button>
</action>
