<action resourceBundle="rfq.rfqNLS"
	formName = "rfqListForm" 
         helpKey="MC.rfq.RFQResponseProductSpecificationList.Help"
	>
  
  <parameter
        listsize="15"
        startindex="0"
        endindex="0"
  />

  <scrollcontrol
        title="resprdatrrespnd"
	display="true"/>
  

  <controlpanel
        display="true"/>
  

  <button>
     <menu name="change"
       	action="basefrm.changeAttribute()"
       	selection="single"/>
     
  
     <menu name="rfqclear"
       	action="basefrm.cleanAttribute()"
