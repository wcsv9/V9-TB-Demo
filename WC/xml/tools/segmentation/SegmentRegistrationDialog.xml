<universalDialog resourceBundle="segmentation.resources"
                 finishURL="SegmentSave"
                 XMLParameter="true"
                 formName="segmentForm"
                 panelMode="true"
                 includeCalendar="true"
                 tableProperty="border=0 cellpadding=1 cellspacing=0 width=100%"
                 pageTitle="registrationUDTitle">

  <include page="/tools/segmentation/RegistrationUDInit.jsp" />

  <jsFile src="/wcs/javascript/tools/common/Util.js"/>
  <jsFile src="/wcs/javascript/tools/segmentation/SegmentNotebook.js"/>

  <section name="registration" sectionTitle="registrationUDTitle" enabled="true" expand="false">

    <element name="registrationStatus"
             type="custom"
             page="/tools/segmentation/RegistrationStatusPanel.jsp">
    </element>

    <element name="registrationDate"
             type="custom"
             page="/tools/segmentation/RegistrationDatePanel.jsp">
    </element>

    <element name="registrationChangeDate"
             type="custom"
             page="/tools/segmentation/RegistrationChangeDatePanel.jsp">
    </element>

    <element name="interests"
             type="custom"
             page="/tools/segmentation/InterestsPanel.jsp">
    </element>

    <element name="company"
             type="custom"
             page="/tools/segmentation/CompanyPanel.jsp">
    </element>

    <element name="currency"
             type="custom"
             page="/tools/segmentation/CurrencyPanel.jsp">
    </element>

    <element name="language"
             type="custom"
             page="/tools/segmentation/LanguagePanel.jsp">
    </element>

    <element name="jobFunction"
             type="custom"
             page="/tools/segmentation/JobFunctionPanel.jsp">
    </element>

    <element name="preferredCommunication"
             type="custom"
             page="/tools/segmentation/PreferredCommunicationPanel.jsp">
    </element>

  </section>

</universalDialog>
