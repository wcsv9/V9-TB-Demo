<dialog   resourceBundle="store.opencloseNLS"
          windowTitle="opencloseTitle"
          finishConfirmation="finishConfirmation"
          cancelConfirmation="cancelConfirmation"
          finishURL="" >
             
  <panel name="opencloseTitle"
         url="StoreOpenCloseView"
         parameters="actionPerformed"
         helpKey="MC.store.openclose.Help"
         hasFinish="NO"
         hasCancel="NO" />
			 
  <button name="opencloseLaunchStore"
	 action="self.CONTENTS.launchStore()" />

  <button name="Cancel"
	 action="self.CONTENTS.cancelAction()" />
</dialog>
