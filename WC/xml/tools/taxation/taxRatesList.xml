<action resourceBundle="taxation.taxationNLS"
	formName="taxRateContentForm" >

	<parameter listsize="20"
		startindex="0"
		resultssize="0"
		orderby="" />

	<scrollcontrol title="taxRatesListTitle"
		display="false" />

	<controlpanel display="true" />

	<button>
		<menu name="Add"
			action="basefrm.addTaxRates()" />
		<menu name="Change"
			action="basefrm.changeTaxRates()"
			selection="single" />
		<menu name="Remove"
			action="basefrm.deleteTaxRates()"
			selection="multiple" />

	</button>

</action>