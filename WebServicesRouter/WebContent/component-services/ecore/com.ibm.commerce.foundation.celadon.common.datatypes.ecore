  <eClassifiers xsi:type="ecore:EClass" name="ComponentStatusRecord">
    <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
      <details key="documentation" value="Indicates the status of a software or hardware component, reported at a given point in time - note that the RecordID member is the key member and identifies a particular reporting of the component status -- the ComponentID member identifies the component for whom a report is being issued or maintained -- this allows the option of multiple status records to be managed for a single component, reflecting the status of that component at different points in time (if desired) -- it may also be the policy of applications to maintain only one status record per component, reflecting only the most recent status report. Note that the status is represented by a two members which express logical status -- StatusCodeMajor (e.g. &quot;OK&quot;, &quot;ERROR&quot;, &quot;WARNING&quot;) and StatusCodeMinor (e.g. &quot;WEAK SIGNAL&quot;) plus a numerical StatusValue member which might be used to represent a component status that changes along a spectrum (e.g. the signal strength reading for a modem). The application should specify its own conventions about reasonable values and meanings for the status fields."/>
    </eAnnotations>
    <eAnnotations>
      <details key="appinfo"/>
    </eAnnotations>
    <eAnnotations source="http:///org/eclipse/emf/ecore/util/ExtendedMetaData">
      <details key="name" value="ComponentStatusRecord"/>
      <details key="kind" value="elementOnly"/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="recordID" eType="ecore:EDataType http://www.eclipse.org/emf/2003/XMLType#//String">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Unique identifier for this status report -- note that this is distinct from the ComponentID member -- multiple status reports may exist for the same component."/>
      </eAnnotations>
      <eAnnotations source="http:///org/eclipse/emf/ecore/util/ExtendedMetaData">
        <details key="kind" value="element"/>
        <details key="name" value="RecordID"/>
        <details key="namespace" value="##targetNamespace"/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="componentID" eType="ecore:EDataType http://www.eclipse.org/emf/2003/XMLType#//String">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Identifies the software or hardware component whose status is being reported -- this should be a globally unique identifier."/>
      </eAnnotations>
      <eAnnotations source="http:///org/eclipse/emf/ecore/util/ExtendedMetaData">
        <details key="kind" value="element"/>
        <details key="name" value="ComponentID"/>
        <details key="namespace" value="##targetNamespace"/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="statusCodeMajor" eType="ecore:EDataType http://www.eclipse.org/emf/2003/XMLType#//String">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Overall status of the component expressed as a logical major code -- typically this is a short string such as &quot;OK&quot;, &quot;IDLE&quot;, &quot;WARNING&quot; or &quot;ERROR&quot;, &quot;STARTING&quot;, &quot;ACTIVITY&quot;, and &quot;STOPPING&quot;."/>
      </eAnnotations>
      <eAnnotations source="http:///org/eclipse/emf/ecore/util/ExtendedMetaData">
        <details key="kind" value="element"/>
        <details key="name" value="StatusCodeMajor"/>
        <details key="namespace" value="##targetNamespace"/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="statusCodeMinor" eType="ecore:EDataType http://www.eclipse.org/emf/2003/XMLType#//String">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Specific status of the component expressed as a logical minor code which complements the major code -- for example, a major status code of  &quot;WARNING&quot; might be coupled with a minor status code of &quot;WEAK SIGNAL&quot;."/>
      </eAnnotations>
      <eAnnotations source="http:///org/eclipse/emf/ecore/util/ExtendedMetaData">
        <details key="kind" value="element"/>
        <details key="name" value="StatusCodeMinor"/>
        <details key="namespace" value="##targetNamespace"/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="statusValue" eType="ecore:EDataType http://www.eclipse.org/emf/2003/XMLType#//Double"
        unsettable="true">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Status of the component expressed as a numerical value -- this is useful for components whose activities may be gauged across a numerical spectrum (e.g. a modem may indicate its signal strength in a status report)."/>
      </eAnnotations>
      <eAnnotations source="http:///org/eclipse/emf/ecore/util/ExtendedMetaData">
        <details key="kind" value="element"/>
        <details key="name" value="StatusValue"/>
