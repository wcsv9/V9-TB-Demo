        unsettable="true">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Periodicity of modem operating status checks -- a typical setting is 10000 (10 seconds)."/>
      </eAnnotations>
      <eAnnotations source="http:///org/eclipse/emf/ecore/util/ExtendedMetaData">
        <details key="kind" value="element"/>
        <details key="name" value="OperatingStatusCheckPeriodMilliseconds"/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="accountStatusCheckPeriodMilliseconds"
        lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2003/XMLType#//Long"
        unsettable="true">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Periodicity of modem account status checks. If the modem is found to be operating normally and has a good signal, but messages cannot be transmitted across it,&#xD;&#xA;it is assumed there is a problem with the GSM account be used by the modem. In this mode, the modem operations are suspended, but attempts will be periodically&#xD;&#xA;made to resume operations using this time interval -- generally one or more hours -- a typical setting is 3600000 (1 hour). "/>
      </eAnnotations>
