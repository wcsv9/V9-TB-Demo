<schema xmlns="http://www.w3.org/2001/XMLSchema" xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore"
	xmlns:oa="http://www.openapplications.org/oagis/9"
	xmlns:_cnt="http://www.ibm.com/xmlns/prod/commerce/9/content"	
	attributeFormDefault="unqualified" elementFormDefault="qualified" 
	targetNamespace="http://www.ibm.com/xmlns/prod/commerce/9/content"
	ecore:nsPrefix="_cnt" 
	ecore:package="com.ibm.commerce.content.facade.datatypes">

	<annotation>
		<documentation>
			This XML schema file defines the AcknowledgeContentVersion BOD which is used to acknowledge
			the ProcessContentVersion BOD.
		</documentation>
	</annotation>

	<import namespace="http://www.openapplications.org/oagis/9"
		schemaLocation="../../../../Resources/Components/Common/Meta.xsd" />					

	<include schemaLocation="../Resources/Nouns/ContentVersion.xsd" />

	<element name="AcknowledgeContentVersion" type="_cnt:AcknowledgeContentVersionType">
		<annotation>
			<documentation>
				Element definition of the AcknowledgeContentVersion BOD.
			</documentation>
		</annotation>
	</element>

	<complexType name="AcknowledgeContentVersionType">
		<annotation>
			<documentation>
				Type definition of the AcknowledgeContentVersion BOD.
			</documentation>
		</annotation>
		<complexContent>
			<extension base="oa:BusinessObjectDocumentType">
				<sequence>
					<element name="DataArea" type="_cnt:AcknowledgeContentVersionDataAreaType">
						<annotation>
							<documentation>Data area of the AcknowledgeContentVersion BOD.</documentation>
						</annotation>
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="AcknowledgeContentVersionDataAreaType">
		<annotation>
			<documentation>Type definition of the AcknowledgeContentVersion BOD's data area.</documentation>
		</annotation>
		<sequence>
			<element ref="oa:Acknowledge">
				<annotation>
					<documentation>The Acknowledge verb.</documentation>
				</annotation>
			</element>
			<element minOccurs="0" maxOccurs="unbounded" ref="_cnt:ContentVersion">
				<annotation>
					<documentation>The ContentVersion noun.</documentation>
				</annotation>
			</element>
		</sequence>
	</complexType>

</schema>