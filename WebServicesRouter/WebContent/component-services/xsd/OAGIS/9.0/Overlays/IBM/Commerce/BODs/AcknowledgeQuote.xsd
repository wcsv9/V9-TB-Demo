<schema xmlns="http://www.w3.org/2001/XMLSchema" xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore"
	xmlns:oa="http://www.openapplications.org/oagis/9"
	xmlns:_ord="http://www.ibm.com/xmlns/prod/commerce/9/order"	
	attributeFormDefault="unqualified" elementFormDefault="qualified" 
	targetNamespace="http://www.ibm.com/xmlns/prod/commerce/9/order"
	ecore:nsPrefix="_ord" 
	ecore:package="com.ibm.commerce.order.facade.datatypes">

	<annotation>
		<documentation>
			This XML schema file defines the AcknowledgeQuote BOD which is used to acknowledge
			the ProcessQuote BOD.
		</documentation>
	</annotation>

	<import namespace="http://www.openapplications.org/oagis/9"
		schemaLocation="../../../../Resources/Components/Common/Meta.xsd" />

	<include schemaLocation="../Resources/Nouns/Quote.xsd" />

	<element name="AcknowledgeQuote" type="_ord:AcknowledgeQuoteType">
		<annotation>
			<documentation>
				Element definition of the AcknowledgeQuote BOD.
			</documentation>
		</annotation>
	</element>

	<complexType name="AcknowledgeQuoteType">
		<annotation>
			<documentation>
				Type definition of the AcknowledgeQuote BOD.
			</documentation>
		</annotation>
		<complexContent>
			<extension base="oa:BusinessObjectDocumentType">
				<sequence>
					<element name="DataArea" type="_ord:AcknowledgeQuoteDataAreaType">
						<annotation>
							<documentation>Data area of the AcknowledgeQuote BOD.</documentation>
						</annotation>
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="AcknowledgeQuoteDataAreaType">
		<annotation>
			<documentation>Type definition of the AcknowledgeQuote BOD's data area.</documentation>
		</annotation>
		<sequence>
			<element ref="oa:Acknowledge">
				<annotation>
					<documentation>The Acknowledge verb.</documentation>
				</annotation>
			</element>
			<element minOccurs="0" maxOccurs="unbounded" ref="_ord:Quote">
				<annotation>
					<documentation>The Quote noun.</documentation>
				</annotation>
			</element>
		</sequence>
	</complexType>

</schema>
