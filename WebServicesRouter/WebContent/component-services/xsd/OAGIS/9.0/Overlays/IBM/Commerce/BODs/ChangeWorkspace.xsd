<schema xmlns="http://www.w3.org/2001/XMLSchema" xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore"
	xmlns:oa="http://www.openapplications.org/oagis/9"
	xmlns:_cnt="http://www.ibm.com/xmlns/prod/commerce/9/content"	
	attributeFormDefault="unqualified" elementFormDefault="qualified" 
	targetNamespace="http://www.ibm.com/xmlns/prod/commerce/9/content"
	ecore:nsPrefix="_cnt" 
	ecore:package="com.ibm.commerce.content.facade.datatypes">

	<annotation>
		<documentation>
			This XML schema file defines the ChangeWorkspace BOD which is used to change one or more workspaces.
		</documentation>
	</annotation>

	<import namespace="http://www.openapplications.org/oagis/9"
		schemaLocation="../../../../Resources/Components/Common/Meta.xsd" />

	<include schemaLocation="../Resources/Nouns/Workspace.xsd" />

	<element name="ChangeWorkspace" type="_cnt:ChangeWorkspaceType">
		<annotation>
			<documentation>
				Element definition of the ChangeWorkspace BOD.
			</documentation>
		</annotation>
	</element>

	<complexType name="ChangeWorkspaceType">
		<annotation>
			<documentation>
				Type definition of the ChangeWorkspace BOD.
			</documentation>
		</annotation>
		<complexContent>
			<extension base="oa:BusinessObjectDocumentType">
				<sequence>
					<element name="DataArea" type="_cnt:ChangeWorkspaceDataAreaType">
						<annotation>
							<documentation>Data area of the ChangeWorkspace BOD.</documentation>
						</annotation>
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="ChangeWorkspaceDataAreaType">
		<annotation>
			<documentation>Type definition of the ChangeWorkspace BOD's data area.</documentation>
		</annotation>
		<sequence>
			<element ref="oa:Change">
				<annotation>
					<documentation>The Change verb.</documentation>
				</annotation>
			</element>
			<element ref="_cnt:Workspace" maxOccurs="unbounded" minOccurs="0">
				<annotation>
					<documentation>The Workspace noun.</documentation>
				</annotation>
			</element>
		</sequence>
	</complexType>

</schema>
