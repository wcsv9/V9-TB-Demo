<schema xmlns="http://www.w3.org/2001/XMLSchema" xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore"
	xmlns:oa="http://www.openapplications.org/oagis/9"
	xmlns:_cat="http://www.ibm.com/xmlns/prod/commerce/9/catalog"	
	attributeFormDefault="unqualified" elementFormDefault="qualified" 
	targetNamespace="http://www.ibm.com/xmlns/prod/commerce/9/catalog"
	ecore:nsPrefix="_cat" 
	ecore:package="com.ibm.commerce.catalog.facade.datatypes">

	<annotation>
		<documentation>
			This XML schema file defines the standard OAGIS business object document used for retrieving one or more catalog nouns.
			
		</documentation>
	</annotation>

	<import namespace="http://www.openapplications.org/oagis/9"
		schemaLocation="../../../../Resources/Components/Common/Meta.xsd" />

	<include schemaLocation="../Resources/Nouns/Catalog.xsd" />

	<element name="GetCatalog" type="_cat:GetCatalogType">
		<annotation>
			<documentation>
				Element definition of the GetCatalog BOD.
			</documentation>
		</annotation>
	</element>

	<complexType name="GetCatalogType">
		<annotation>
			<documentation>
				Type definition of the GetCatalog BOD.
			</documentation>
		</annotation>
		<complexContent>
			<extension base="oa:BusinessObjectDocumentType">
				<sequence>
					<element name="DataArea" type="_cat:GetCatalogDataAreaType">
						<annotation>
							<documentation>Data area of the GetCatalog BOD.</documentation>
						</annotation>
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="GetCatalogDataAreaType">
		<annotation>
			<documentation>Type definition of the GetCatalog BOD's data area.</documentation>
		</annotation>
		<sequence>
			<element ref="oa:Get">
				<annotation>
					<documentation>The Get verb.</documentation>
				</annotation>
			</element>
			<element ref="_cat:Catalog" minOccurs="0" maxOccurs="unbounded">
				<annotation>
					<documentation>The Catalog noun.</documentation>
				</annotation>
			</element>
		</sequence>
	</complexType>

</schema>

