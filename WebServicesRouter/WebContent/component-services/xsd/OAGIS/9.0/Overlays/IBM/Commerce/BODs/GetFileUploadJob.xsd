<schema xmlns="http://www.w3.org/2001/XMLSchema" xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore"
	xmlns:oa="http://www.openapplications.org/oagis/9"
	xmlns:_cnt="http://www.ibm.com/xmlns/prod/commerce/9/content"	
	attributeFormDefault="unqualified" elementFormDefault="qualified" 
	targetNamespace="http://www.ibm.com/xmlns/prod/commerce/9/content"
	ecore:nsPrefix="_cnt" 
	ecore:package="com.ibm.commerce.content.facade.datatypes">

	<annotation>
		<documentation>
			<![CDATA[
				<Service name="GetFileUploadJob">
					<ActionCode></ActionCode>
					<ActionExpression></ActionExpression>
					<Description>
							Get the FileUploadJob nouns
					</Description>
				</Service>
			]]>
		</documentation>
	</annotation>

	<import namespace="http://www.openapplications.org/oagis/9"
		schemaLocation="../../../../Resources/Components/Common/Meta.xsd" />

	<include schemaLocation="../Resources/Nouns/FileUploadJob.xsd" />

	<element name="GetFileUploadJob" type="_cnt:GetFileUploadJobType">
		<annotation>
			<documentation>
				Element definition of the GetFileUploadJob BOD.
			</documentation>
		</annotation>
	</element>

	<complexType name="GetFileUploadJobType">
		<annotation>
			<documentation>
				Type definition of the GetFileUploadJob BOD.
			</documentation>
		</annotation>
		<complexContent>
			<extension base="oa:BusinessObjectDocumentType">
				<sequence>
					<element name="DataArea" type="_cnt:GetFileUploadJobDataAreaType">
						<annotation>
							<documentation>Data area of the GetFileUploadJob BOD.</documentation>
						</annotation>
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="GetFileUploadJobDataAreaType">
		<annotation>
			<documentation>Type definition of the GetFileUploadJob BOD's data area.</documentation>
		</annotation>
		<sequence>
			<element ref="oa:Get">
				<annotation>
					<documentation>The Get verb.</documentation>
				</annotation>
			</element>
			<element ref="_cnt:FileUploadJob" minOccurs="0" maxOccurs="unbounded">
				<annotation>
					<documentation>The FileUploadJob noun.</documentation>
				</annotation>
			</element>
		</sequence>
	</complexType>

</schema>

