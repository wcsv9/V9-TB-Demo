<schema xmlns="http://www.w3.org/2001/XMLSchema" xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore"
	xmlns:oa="http://www.openapplications.org/oagis/9"
	xmlns:_sto="http://www.ibm.com/xmlns/prod/commerce/9/store"	
	attributeFormDefault="unqualified" elementFormDefault="qualified" 
	targetNamespace="http://www.ibm.com/xmlns/prod/commerce/9/store"
	ecore:nsPrefix="_sto" 
	ecore:package="com.ibm.commerce.store.facade.datatypes">

	<annotation>
		<documentation>
			<![CDATA[
				<Service name="GetGeoNode">
					<ActionCode></ActionCode>
					<ActionExpression></ActionExpression>
					<Description>
							Get the GeoNode nouns
					</Description>
				</Service>
			]]>
		</documentation>
	</annotation>

	<import namespace="http://www.openapplications.org/oagis/9"
		schemaLocation="../../../../Resources/Components/Common/Meta.xsd" />

	<include schemaLocation="../Resources/Nouns/GeoNode.xsd" />

	<element name="GetGeoNode" type="_sto:GetGeoNodeType">
		<annotation>
			<documentation>
				Element definition of the GetGeoNode BOD.
			</documentation>
		</annotation>
	</element>

	<complexType name="GetGeoNodeType">
		<annotation>
			<documentation>
				Type definition of the GetGeoNode BOD.
			</documentation>
		</annotation>
		<complexContent>
			<extension base="oa:BusinessObjectDocumentType">
				<sequence>
					<element name="DataArea" type="_sto:GetGeoNodeDataAreaType">
						<annotation>
							<documentation>Data area of the GetGeoNode BOD.</documentation>
						</annotation>
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="GetGeoNodeDataAreaType">
		<annotation>
			<documentation>Type definition of the GetGeoNode BOD's data area.</documentation>
		</annotation>
		<sequence>
			<element ref="oa:Get">
				<annotation>
					<documentation>The Get verb.</documentation>
				</annotation>
			</element>
			<element ref="_sto:GeoNode" minOccurs="0" maxOccurs="unbounded">
				<annotation>
					<documentation>The GeoNode noun.</documentation>
				</annotation>
			</element>
		</sequence>
	</complexType>

</schema>

