<schema xmlns="http://www.w3.org/2001/XMLSchema" xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore"
	xmlns:oa="http://www.openapplications.org/oagis/9"
	xmlns:_cnt="http://www.ibm.com/xmlns/prod/commerce/9/content"	
	attributeFormDefault="unqualified" elementFormDefault="qualified" 
	targetNamespace="http://www.ibm.com/xmlns/prod/commerce/9/content"
	ecore:nsPrefix="_cnt" 
	ecore:package="com.ibm.commerce.content.facade.datatypes">

	<annotation>
		<documentation>
			This XML schema file defines the standard OAGIS business object document used for retrieving one or more task group nouns.
		</documentation>
	</annotation>

	<import namespace="http://www.openapplications.org/oagis/9"
		schemaLocation="../../../../Resources/Components/Common/Meta.xsd" />

	<include schemaLocation="../Resources/Nouns/TaskGroup.xsd" />

	<element name="GetTaskGroup" type="_cnt:GetTaskGroupType">
		<annotation>
			<documentation>
				Element definition of the GetTaskGroup BOD.
			</documentation>
		</annotation>
	</element>

	<complexType name="GetTaskGroupType">
		<annotation>
			<documentation>
				Type definition of the GetTaskGroup BOD.
			</documentation>
		</annotation>
		<complexContent>
			<extension base="oa:BusinessObjectDocumentType">
				<sequence>
					<element name="DataArea" type="_cnt:GetTaskGroupDataAreaType">
						<annotation>
							<documentation>Data area of the GetTaskGroup BOD.</documentation>
						</annotation>
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="GetTaskGroupDataAreaType">
		<annotation>
			<documentation>Type definition of the GetTaskGroup BOD's data area.</documentation>
		</annotation>
		<sequence>
			<element ref="oa:Get">
				<annotation>
					<documentation>The Get verb.</documentation>
				</annotation>
			</element>
			<element ref="_cnt:TaskGroup" minOccurs="0" maxOccurs="unbounded">
				<annotation>
					<documentation>The TaskGroup noun.</documentation>
				</annotation>
			</element>
		</sequence>
	</complexType>

</schema>

