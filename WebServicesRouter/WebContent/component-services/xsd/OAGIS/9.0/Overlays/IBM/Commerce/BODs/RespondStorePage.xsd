<schema xmlns="http://www.w3.org/2001/XMLSchema" xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore"
	xmlns:oa="http://www.openapplications.org/oagis/9"
	xmlns:_ifr="http://www.ibm.com/xmlns/prod/commerce/9/infrastructure"	
	attributeFormDefault="unqualified" elementFormDefault="qualified" 
	targetNamespace="http://www.ibm.com/xmlns/prod/commerce/9/infrastructure"
	ecore:nsPrefix="_ifr" 
	ecore:package="com.ibm.commerce.infrastructure.facade.datatypes">

	<annotation>
		<documentation>
			This StorePage XML schema file defines the AcknowledgeStorePage BOD which is used to acknowledge
			the ProcessStorePage BOD.
		</documentation>
	</annotation>

	<import namespace="http://www.openapplications.org/oagis/9"
		schemaLocation="../../../../Resources/Components/Common/Meta.xsd" />

	<include schemaLocation="../Resources/Nouns/StorePage.xsd" />

	<element name="RespondStorePage" type="_ifr:RespondStorePageType">
		<annotation>
			<documentation>
				Element definition of the AcknowledgeStorePage BOD.
			</documentation>
		</annotation>
	</element>

	<complexType name="RespondStorePageType">
		<annotation>
			<documentation>
				Type definition of the AcknowledgeStorePage BOD.
			</documentation>
		</annotation>
		<complexContent>
			<extension base="oa:BusinessObjectDocumentType">
				<sequence>
					<element name="DataArea" type="_ifr:RespondStorePageDataAreaType">
						<annotation>
							<documentation>Data area of the AcknowledgeStorePage BOD.</documentation>
						</annotation>
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="RespondStorePageDataAreaType">
		<annotation>
			<documentation>Type definition of the AcknowledgeStorePage BOD's data area.</documentation>
		</annotation>
		<sequence>
			<element ref="oa:Respond">
				<annotation>
					<documentation>The Acknowledge verb.</documentation>
				</annotation>
			</element>
			<element maxOccurs="unbounded" ref="_ifr:StorePage">
				<annotation>
					<documentation>The StorePage noun.</documentation>
				</annotation>
			</element>
		</sequence>
	</complexType>

</schema>
