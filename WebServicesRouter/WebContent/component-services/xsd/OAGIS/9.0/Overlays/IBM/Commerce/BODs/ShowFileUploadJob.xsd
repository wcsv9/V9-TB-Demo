<schema xmlns="http://www.w3.org/2001/XMLSchema" xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore"
	xmlns:oa="http://www.openapplications.org/oagis/9"
	xmlns:_cnt="http://www.ibm.com/xmlns/prod/commerce/9/content"	
	attributeFormDefault="unqualified" elementFormDefault="qualified" 
	targetNamespace="http://www.ibm.com/xmlns/prod/commerce/9/content"
	ecore:nsPrefix="_cnt" 
	ecore:package="com.ibm.commerce.content.facade.datatypes">

	<annotation>
		<documentation>
			This FileUploadJob XML schema file defines the ShowFileUploadJob BOD which is used to show an fileuploadjob or
			list of FileUploadJobs.
		</documentation>
	</annotation>

	<import namespace="http://www.openapplications.org/oagis/9"
		schemaLocation="../../../../Resources/Components/Common/Meta.xsd" />

	<include schemaLocation="../Resources/Nouns/FileUploadJob.xsd" />

	<element name="ShowFileUploadJob" type="_cnt:ShowFileUploadJobType">
		<annotation>
			<documentation>Element definition of the ShowFileUploadJob BOD.</documentation>
		</annotation>
	</element>

	<complexType name="ShowFileUploadJobType">
		<annotation>
			<documentation>Type definition of the ShowFileUploadJob BOD.</documentation>
		</annotation>
		<complexContent>
			<extension base="oa:BusinessObjectDocumentType">
				<sequence>
					<element name="DataArea" type="_cnt:ShowFileUploadJobDataAreaType">
						<annotation>
							<documentation>Data area of the ShowFileUploadJob BOD.</documentation>
						</annotation>
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="ShowFileUploadJobDataAreaType">
		<annotation>
			<documentation>Type definition of the ShowFileUploadJob BOD's data area.</documentation>
		</annotation>
		<sequence>
			<element ref="oa:Show">
				<annotation>
					<documentation>The Show verb.</documentation>
				</annotation>
			</element>
			<element ref="_cnt:FileUploadJob" minOccurs="0" maxOccurs="unbounded">
				<annotation>
					<documentation>The FileUploadJob noun.</documentation>
				</annotation>
			</element>
		</sequence>
	</complexType>

</schema>
