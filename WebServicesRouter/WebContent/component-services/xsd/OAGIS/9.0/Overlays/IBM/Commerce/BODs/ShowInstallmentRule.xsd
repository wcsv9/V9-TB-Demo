<schema xmlns="http://www.w3.org/2001/XMLSchema" xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore"
	xmlns:oa="http://www.openapplications.org/oagis/9"
	xmlns:_ins="http://www.ibm.com/xmlns/prod/commerce/9/installment"	
	attributeFormDefault="unqualified" elementFormDefault="qualified" 
	targetNamespace="http://www.ibm.com/xmlns/prod/commerce/9/installment"
	ecore:nsPrefix="_ins" 
	ecore:package="com.ibm.commerce.installment.facade.datatypes">

	<annotation>
		<documentation>
			This InstallmentRule XML schema file defines the ShowInstallmentRule BOD which is used to show an installmentrule or
			list of InstallmentRules.
		</documentation>
	</annotation>

	<import namespace="http://www.openapplications.org/oagis/9"
		schemaLocation="../../../../Resources/Components/Common/Meta.xsd" />

	<include schemaLocation="../Resources/Nouns/InstallmentRule.xsd" />

	<element name="ShowInstallmentRule" type="_ins:ShowInstallmentRuleType">
		<annotation>
			<documentation>Element definition of the ShowInstallmentRule BOD.</documentation>
		</annotation>
	</element>

	<complexType name="ShowInstallmentRuleType">
		<annotation>
			<documentation>Type definition of the ShowInstallmentRule BOD.</documentation>
		</annotation>
		<complexContent>
			<extension base="oa:BusinessObjectDocumentType">
				<sequence>
					<element name="DataArea" type="_ins:ShowInstallmentRuleDataAreaType">
						<annotation>
							<documentation>Data area of the ShowInstallmentRule BOD.</documentation>
						</annotation>
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="ShowInstallmentRuleDataAreaType">
		<annotation>
			<documentation>Type definition of the ShowInstallmentRule BOD's data area.</documentation>
		</annotation>
		<sequence>
			<element ref="oa:Show">
				<annotation>
					<documentation>The Show verb.</documentation>
				</annotation>
			</element>
			<element ref="_ins:InstallmentRule" minOccurs="0" maxOccurs="unbounded">
				<annotation>
					<documentation>The InstallmentRule noun.</documentation>
				</annotation>
			</element>
		</sequence>
	</complexType>

</schema>
