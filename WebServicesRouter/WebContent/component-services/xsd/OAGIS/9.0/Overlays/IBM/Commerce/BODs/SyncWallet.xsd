<schema xmlns="http://www.w3.org/2001/XMLSchema" xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" xmlns:oa="http://www.openapplications.org/oagis/9" xmlns:_wal="http://www.ibm.com/xmlns/prod/commerce/9/wallet"
	attributeFormDefault="unqualified" elementFormDefault="qualified" targetNamespace="http://www.ibm.com/xmlns/prod/commerce/9/wallet" ecore:nsPrefix="_wal" ecore:package="com.ibm.commerce.wallet.facade.datatypes">

	<annotation>
		<documentation>
			<![CDATA[
				<Service name="SyncWallet">
					<ActionCode></ActionCode>
					<ActionExpression></ActionExpression>
					<Description>
							Sync the Wallet nouns
					</Description>
				</Service>
			]]>
		</documentation>
	</annotation>

	<import namespace="http://www.openapplications.org/oagis/9" schemaLocation="../../../../Resources/Components/Common/Meta.xsd"/>

	<include schemaLocation="../Resources/Nouns/Wallet.xsd"/>

	<element name="SyncWallet" type="_wal:SyncWalletType">
		<annotation>
			<documentation>
				Element definition of the SyncWallet BOD.
			</documentation>
		</annotation>
	</element>

	<complexType name="SyncWalletType">
		<annotation>
			<documentation>
				Type definition of the SyncWallet BOD.
			</documentation>
		</annotation>
		<complexContent>
			<extension base="oa:BusinessObjectDocumentType">
				<sequence>
					<element name="DataArea" type="_wal:SyncWalletDataAreaType">
						<annotation>
							<documentation>Data area of the SyncWallet BOD.</documentation>
						</annotation>
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="SyncWalletDataAreaType">
		<annotation>
			<documentation>Type definition of the SyncWallet BOD's data area.</documentation>
		</annotation>
		<sequence>
			<element ref="oa:Sync">
				<annotation>
					<documentation>The Sync verb.</documentation>
				</annotation>
			</element>
			<element minOccurs="0" maxOccurs="unbounded" ref="_wal:Wallet">
				<annotation>
					<documentation>The Wallet noun.</documentation>
				</annotation>
			</element>
		</sequence>
	</complexType>

</schema>
