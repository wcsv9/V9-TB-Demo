				minOccurs="0" maxOccurs="1">
				<annotation>
					<documentation>The identifier of the flow element.
					</documentation>
				</annotation>
			</element>
			<element name="Description" type="string" minOccurs="0"
				maxOccurs="1">
				<annotation>
					<documentation>The description of the flow element.
					</documentation>
				</annotation>
			</element>


			<element name="ParentElementIdentifier" type="_wcf:FlowElementIdentifierType"
				minOccurs="0" maxOccurs="1">
				<annotation>
					<documentation>The parent identifier of the price element. Every price element has only one direct parent, but the root price element has no parent.
					</documentation>
				</annotation>
			</element>
			<element name="ElementSequence" type="string" minOccurs="0"
				maxOccurs="1">
				<annotation>
					<documentation>The sequence of the element under the same parent. Each flow has root path which other elements are placed on. The sequence specify the execution order of the elements on the path.
					</documentation>
				</annotation>
			</element>
			<element name="ElementAttribute" type="_wcf:FlowElementAttributeType"
				minOccurs="0" maxOccurs="unbounded">
				<annotation>
					<documentation>The attribute name and value associated with a flow element.
					</documentation>
				</annotation>
			</element>
			<element ref="_wcf:UserData" minOccurs="0" maxOccurs="1">
				<annotation>
					<documentation>User Data.</documentation>
				</annotation>
			</element>
