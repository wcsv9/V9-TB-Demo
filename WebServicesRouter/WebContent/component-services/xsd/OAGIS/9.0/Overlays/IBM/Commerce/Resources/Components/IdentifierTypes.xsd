<schema xmlns="http://www.w3.org/2001/XMLSchema" xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore"
	xmlns:oa="http://www.openapplications.org/oagis/9"
	xmlns:_wcf="http://www.ibm.com/xmlns/prod/commerce/9/foundation"
	targetNamespace="http://www.ibm.com/xmlns/prod/commerce/9/foundation"
	elementFormDefault="qualified"
	attributeFormDefault="unqualified" 
	ecore:nsPrefix="_wcf"
	ecore:package="com.ibm.commerce.foundation.common.datatypes">

	<annotation>
		<documentation>
			This XML schema file defines the common elements and types shared by the
			entire schema.
		</documentation>
	</annotation>

<!-- 
	Identifier types for member component
-->

	<complexType name="PersonExternalIdentifierType">
		<annotation>
			<documentation>The type definition of an external identifier for a person.</documentation>
		</annotation>
		<sequence>
			<element name="Identifier" type="string" minOccurs="0" maxOccurs="1">
				<annotation>
					<documentation>An external identifier for a person, for example logon ID.</documentation>
				</annotation>
			</element>
		</sequence>
	</complexType>
	
	<complexType name="PersonIdentifierType">
		<annotation>
			<documentation>The type definition of a person identifier.</documentation>
		</annotation>
		<sequence>
			<element maxOccurs="1" minOccurs="0" name="UniqueID"
				type="string">
				<annotation>
					<documentation>The unique identifier of the person.</documentation>
				</annotation>
			</element>
			<element maxOccurs="1" minOccurs="0"
				name="DistinguishedName" type="string">
				<annotation>
					<documentation>The distinguished name of the person.</documentation>
				</annotation>
			</element>
			<element name="ExternalIdentifier" type="_wcf:PersonExternalIdentifierType" minOccurs="0" maxOccurs="1">
				<annotation>
					<documentation>An external identifier of the person.</documentation>
				</annotation>
			</element>
		</sequence>
	</complexType>

	<complexType name="OrganizationIdentifierType">
		<annotation>
			<documentation>The type definition of an organization identifier.</documentation>
		</annotation>
		<sequence>
			<element maxOccurs="1" minOccurs="0" name="UniqueID" type="string">
				<annotation>
					<documentation>The unique identifier of the organization.</documentation>
				</annotation>
			</element>
			<element maxOccurs="1" minOccurs="0" name="DistinguishedName" type="string">
				<annotation>
					<documentation>The distinguished name of the organization.</documentation>
				</annotation>
			</element>
		</sequence>
	</complexType>

	<complexType name="ContactInfoExternalIdentifierType">
		<annotation>
			<documentation>The type definition of an external identifier for contact information.</documentation>
		</annotation>
		<sequence>
			<element maxOccurs="1" minOccurs="1" name="ContactInfoNickName" type="string">
				<annotation>
					<documentation>The nickname of the contact information.</documentation>
				</annotation>
			</element>
			<choice>
				<element maxOccurs="1" minOccurs="0" name="PersonIdentifier" type="_wcf:PersonIdentifierType">
					<annotation>
						<documentation>The person that owns the contact information.</documentation>
					</annotation>
				</element>	
				<element maxOccurs="1" minOccurs="0" name="OrganizationIdentifier" type="_wcf:OrganizationIdentifierType">
					<annotation>
						<documentation>The organization that owns the contact information.</documentation>
					</annotation>
				</element>		
			</choice>				
		</sequence>			
	</complexType>		
	
	<complexType name="ContactInfoIdentifierType">
		<annotation>
			<documentation>The type definition of an identifier for contact information.</documentation>
		</annotation>
		<sequence>
			<element maxOccurs="1" minOccurs="0" name="UniqueID" type="string">
				<annotation>
					<documentation>The unique identifier of the contact information.</documentation>
				</annotation>
			</element>
			<element maxOccurs="1" minOccurs="0" name="ExternalIdentifier" type="_wcf:ContactInfoExternalIdentifierType">
				<annotation>
					<documentation>The external identifier of the contact information.</documentation>
				</annotation>
			</element>
		</sequence>
	</complexType>	
	
	<complexType name="MemberGroupExternalIdentifierType">
		<annotation>
			<documentation>The type definition of an external identifier for a member group.</documentation>
		</annotation>
		<sequence>
			<element maxOccurs="1" minOccurs="1" name="Name" type="string">
				<annotation>
					<documentation>The name of the member group.</documentation>
				</annotation>
			</element>
			<element name="StoreIdentifier" type="_wcf:StoreIdentifierType" minOccurs="0" maxOccurs="1">
				<annotation>
					<documentation>The identifier of the store associated with the member group, for example
					a customer segment.</documentation>
				</annotation>
			</element>	
		</sequence>	
		<attribute name="ownerID" type="string" use="optional">
			<annotation>
				<documentation>
					The owner of a member group.
				</documentation>
			</annotation>
		</attribute>		
	</complexType>	
	
	<complexType name="MemberGroupIdentifierType">
		<annotation>
			<documentation>The type definition of an identifier for a member group.</documentation>
		</annotation>
		<sequence>
			<element maxOccurs="1" minOccurs="0" name="UniqueID" type="string">
				<annotation>
					<documentation>The unique identifier of the member group.</documentation>
				</annotation>
			</element>
			<element maxOccurs="1" minOccurs="0" name="ExternalIdentifier" type="_wcf:MemberGroupExternalIdentifierType">
				<annotation>
					<documentation>The external identifier of the member group.</documentation>
				</annotation>
			</element>
		</sequence>
	</complexType>	
	
	<complexType name="RoleIdentifierType">
		<annotation>
			<documentation>The type definition of a role identifier.</documentation>
		</annotation>
		<sequence>
			<element maxOccurs="1" minOccurs="0" name="UniqueID" type="string">
				<annotation>
					<documentation>The unique identifier of the role.</documentation>
				</annotation>
			</element>
			<element maxOccurs="1" minOccurs="0" name="Name" type="string">
				<annotation>
					<documentation>The name of the role.</documentation>
				</annotation>
			</element>
		</sequence>
	</complexType>
	
<!-- 
	Identifier types for catalog component
-->

	<complexType name="CatalogExternalIdentifierType">
		<annotation>
			<documentation>
				A type definition for a unique external identifier for
				the catalog.
			</documentation>
		</annotation>
		<sequence>
			<element minOccurs="1" name="Identifier" type="string">
				<annotation>
					<documentation>
						External identifying name for the catalog.
					</documentation>
				</annotation>
			</element>
			<element name="StoreIdentifier" type="_wcf:StoreIdentifierType" minOccurs="0" maxOccurs="1">
				<annotation>
					<documentation>The identifier of the store associated with the catalog.</documentation>
				</annotation>
			</element>		
		</sequence>
		<attribute name="ownerID" type="string" use="optional">
			<annotation>
				<documentation>
					The owner organization of the catalog.
				</documentation>
			</annotation>
		</attribute>
	</complexType>
	
	<complexType name="CatalogIdentifierType">
		<sequence>
			<element minOccurs="0" name="UniqueID" type="string">
				<annotation>
					<documentation>
						A unique ID for a catalog.
					</documentation>
				</annotation>
			</element>
			<element minOccurs="0" name="ExternalIdentifier"
				type="_wcf:CatalogExternalIdentifierType">
				<annotation>
					<documentation>
						External identifier for the catalog.
					</documentation>
				</annotation>
			</element>
		</sequence>
	</complexType>

	<complexType name="CatalogGroupIdentifierType">
		<annotation>
			<documentation>
				A type definition for a catalog group identifier.
			</documentation>
		</annotation>
		<sequence>
			<element minOccurs="0" name="UniqueID" type="string">
				<annotation>
					<documentation>
						A unique ID for a catalog group.
					</documentation>
				</annotation>
			</element>
			<element minOccurs="0" name="ExternalIdentifier"
				type="_wcf:CatalogGroupExternalIdentifierType">
				<annotation>
					<documentation>
						An external identifier for a catalog group.
					</documentation>
				</annotation>
			</element>
		</sequence>
	</complexType>
	
	<complexType name="CatalogGroupExternalIdentifierType">
		<annotation>
			<documentation>
				A type definition of an unique external identifier for a
				catalog group.
			</documentation>
		</annotation>
		<sequence>
			<element minOccurs="1" name="GroupIdentifier"
				type="string">
				<annotation>
					<documentation>
						External identifying name for the catalog group.
					</documentation>
				</annotation>
			</element>
			<element name="StoreIdentifier" type="_wcf:StoreIdentifierType" minOccurs="0" maxOccurs="1">
				<annotation>
					<documentation>The identifier of the store associated with the catalog group.</documentation>
				</annotation>
			</element>		
		</sequence>
		<attribute name="ownerID" type="string" use="optional">
			<annotation>
				<documentation>
					The owner organization of the catalog group.
				</documentation>
			</annotation>
		</attribute>
	</complexType>
	
	<complexType name="CatalogEntryIdentifierType">
		<annotation>
			<documentation>
				A type definition for a catalog entry identifier.
			</documentation>
		</annotation>
		<sequence>
			<element minOccurs="0" name="UniqueID" type="string">
				<annotation>
					<documentation>
						A unique ID for a catalog entry.
					</documentation>
				</annotation>
			</element>
			<element minOccurs="0" name="ExternalIdentifier"
				type="_wcf:PartNumberIdentifierType">
				<annotation>
					<documentation>
						An external identifier for a catalog entry.
					</documentation>
				</annotation>
			</element>
		</sequence>
	</complexType>
	
	<complexType name="PartNumberIdentifierType">
		<sequence>
			<element minOccurs="1" name="PartNumber" type="string">
				<annotation>
					<documentation>
						The part number or SKU for a catalog entry.
					</documentation>
				</annotation>
			</element>
			<element name="StoreIdentifier" type="_wcf:StoreIdentifierType" minOccurs="0" maxOccurs="1">
				<annotation>
					<documentation>The identifier of the store associated with the catalog entry.</documentation>
				</annotation>
			</element>		
		</sequence>
		<attribute name="ownerID" type="string" use="optional">
			<annotation>
				<documentation>
					The owner organization of the catalog entry.
				</documentation>
			</annotation>
		</attribute>
	</complexType>

	<complexType name="AttributeDictionaryIdentifierType">
		<annotation>
			<documentation>
				A type definition for an attribute dictionary identifier.
			</documentation>
		</annotation>
		<sequence>
			<element minOccurs="0" name="UniqueID" type="string">
				<annotation>
					<documentation>
						A unique ID for an attribute dictionary. Reference to ATTRDICT.ATTRDICT_ID.
					</documentation>
				</annotation>
			</element>
			<element minOccurs="0" name="ExternalIdentifier" type="_wcf:AttributeDictionaryExternalIdentifierType">
				<annotation>
					<documentation>
						An external identifier for an attribute	dictionary.
					</documentation>
				</annotation>
			</element>
		</sequence>			
	</complexType>

	<complexType name="AttributeDictionaryExternalIdentifierType">
		<annotation>
			<documentation>
				An external identifier for an attribute dictionary.			
			</documentation>
		</annotation>
		<sequence>		
			<element name="Identifier" type="string"  minOccurs="1" maxOccurs="1">
				<annotation>
					<documentation>The unique string identifier of the attribute dictionary.</documentation>
				</annotation>
			</element>	
			<element name="StoreIdentifier" type="_wcf:StoreIdentifierType" minOccurs="0" maxOccurs="1">
				<annotation>
				   	<documentation>The identifier of the store that owns this Attribute Dictionary.</documentation>
                </annotation>
			</element>	
		</sequence>		
	</complexType>

	<complexType name="AttributeIdentifierType">
		<annotation>
			<documentation>
				A type definition for an catalog attribute identifier.
			</documentation>
		</annotation>
		<sequence>
			<element name="UniqueID" type="string" minOccurs="0" maxOccurs="1">
				<annotation>
					<documentation>The attribute ID. Reference to ATTR.ATTR_ID.</documentation>
				</annotation>
			</element>
			<element name="ExternalIdentifier" type="_wcf:AttributeExternalIdentifierType" minOccurs="0" maxOccurs="1">
				<annotation>
					<documentation>
						The external identifier for this catalog attribute.
					</documentation>
				</annotation>
			</element>
		</sequence>		
	</complexType>	
	
	<complexType name="AttributeExternalIdentifierType">
		<annotation>
			<documentation>
				An external identifier for an catalog attribute.		
			</documentation>
		</annotation>
		<sequence>
			<element name="AttributeDictionaryIdentifier" type="_wcf:AttributeDictionaryIdentifierType" minOccurs="1" maxOccurs="1">
				<annotation>
					<documentation>
						The identifier of the attribute dictionary to which the attribute belongs.
					</documentation>
				</annotation>
			</element>		
			<element name="Identifier" type="string" minOccurs="1" maxOccurs="1">
				<annotation>
					<documentation>The unique string identifier of the catalog attribute.</documentation>
				</annotation>
			</element>	
			<element name="StoreIdentifier" type="_wcf:StoreIdentifierType" minOccurs="0" maxOccurs="1">
				<annotation>
				   	<documentation>The identifier of the store that owns this catalog attribute.</documentation>
                </annotation>
			</element>	
		</sequence>	
	</complexType>
	
	<complexType name="AttributeGroupIdentifierType">
		<annotation>
			<documentation>
				A type definition for an catalog attribute group identifier.
			</documentation>
		</annotation>
		<sequence>
			<element name="UniqueID" type="string" minOccurs="0" maxOccurs="1">
				<annotation>
					<documentation>The unqiue identifier of the catalog attribute group. Reference to ATTRDICTGRP.ATTRDICTGRP_ID.</documentation>
				</annotation>
			</element>		
			<element name="ExternalIdentifier" type="_wcf:AttributeGroupExternalIdentifierType" minOccurs="0" maxOccurs="1">
				<annotation>
					<documentation>
						The external identifier for this catalog attribute group.
					</documentation>
				</annotation>
			</element>	
		</sequence>
	</complexType>	

	<complexType name="AttributeGroupExternalIdentifierType">
		<annotation>
			<documentation>
				An external identifier for an catalog attribute group.			
			</documentation>
		</annotation>
		<sequence>
			<element name="AttributeDictionaryIdentifier" type="_wcf:AttributeDictionaryIdentifierType" minOccurs="0" maxOccurs="1">
				<annotation>
					<documentation>
						The identifier of the attribute dictionary to which the attribute group belongs. 
					</documentation>
				</annotation>
			</element>		
			<element name="Identifier" type="string" minOccurs="1" maxOccurs="1">
				<annotation>
					<documentation>The unique string identifier of the catalog attribute group.</documentation>
				</annotation>
			</element>	
			<element name="StoreIdentifier" type="_wcf:StoreIdentifierType" minOccurs="0" maxOccurs="1">
                <annotation>
                	<documentation>The identifier of the store that owns this catalog attribute group.</documentation>
                </annotation>
			</element>				
		</sequence>	
	</complexType>

<!-- 
	Identifier types for contract component
-->

	<complexType name="ContractIdentifierType">
		<annotation>
			<documentation>The type definition of an contract identifier.</documentation>
		</annotation>
		<sequence>
			<element maxOccurs="1" minOccurs="0" name="UniqueID" type="string">
				<annotation>
					<documentation>The contract ID.</documentation>
				</annotation>
			</element>
			<element maxOccurs="1" minOccurs="0" name="ExternalIdentifier" type="_wcf:ContractExternalIdentifierType">
				<annotation>
					<documentation>The external contract ID.</documentation>
				</annotation>
			</element>
		</sequence>
	</complexType>

	<complexType name="ContractExternalIdentifierType">
		<annotation>
			<documentation>
				The type definition of an ExternalContractId.
			</documentation>
		</annotation>
		<sequence>
			<element name="Name" type="string"  minOccurs="1">
				<annotation>
					<documentation>The name of the contract.</documentation>
				</annotation>
			</element>	
		</sequence>
		<attribute name="origin" type="string" use="required">
			<annotation>
				<documentation>The Origin of the contract.</documentation>
			</annotation>
		</attribute>		
		<attribute name="majorVersionNumber" type="int" default="1">
			<annotation>
				<documentation>The major version of the contract.</documentation>
			</annotation>
		</attribute>
		<attribute name="minorVersionNumber" type="int" default="0">
			<annotation>
				<documentation>The minor version of the contract.</documentation>
			</annotation>
		</attribute>
		<attribute name="ownerID" type="string" use="required">
			<annotation>
				<documentation>The owner of the contract.</documentation>
			</annotation>
		</attribute>
	</complexType>
       
        <complexType name="PolicyIdentifierType">
		<annotation>
			<documentation>The type definition of a policy identifier.</documentation>
		</annotation>
		<sequence>
			<element maxOccurs="1" minOccurs="0" name="UniqueID" type="string">
				<annotation>
					<documentation>The policy ID.</documentation>
				</annotation>
			</element>
			<element maxOccurs="1" minOccurs="0" name="ExternalIdentifier" type="_wcf:PolicyExternalIdentifierType">
				<annotation>
					<documentation>The external policy ID.</documentation>
				</annotation>
			</element>
		</sequence>
	</complexType>
	
	<complexType name="PolicyExternalIdentifierType">
		<annotation>
			<documentation>
				The type definition of an external policy ID.
			</documentation>
		</annotation>
		<sequence>
			<element name="Name" type="string"  minOccurs="1" maxOccurs="1">
				<annotation>
					<documentation>The name of the policy.</documentation>
				</annotation>
			</element>	
			<element name="Type" type="string"  minOccurs="1" maxOccurs="1">
				<annotation>
					<documentation>The type of the policy.</documentation>
				</annotation>
			</element>
			<element name="StoreIdentifier" type="_wcf:StoreIdentifierType" minOccurs="0" maxOccurs="1">
				<annotation>
					<documentation>The identifier of the store associated with the policy.</documentation>
				</annotation>
			</element>
		</sequence>
	</complexType>
	
	<complexType name="BusinessAccountIdentifierType">
		<annotation>
			<documentation>The type definition of a business account identifier.</documentation>
		</annotation>
		<sequence>
			<element maxOccurs="1" minOccurs="0" name="UniqueID" type="string">
				<annotation>
					<documentation>The business account ID.</documentation>
				</annotation>
			</element>
			<element maxOccurs="1" minOccurs="0" name="ExternalIdentifier" type="_wcf:BusinessAccountExternalIdentifierType">
				<annotation>
					<documentation>The external business account ID.</documentation>
				</annotation>
			</element>
		</sequence>
	</complexType>
	
	<complexType name="BusinessAccountExternalIdentifierType">
		<annotation>
			<documentation>
				The type definition of an external business account ID.
			</documentation>
		</annotation>
		<sequence>
			<element name="Name" type="string"  minOccurs="1">
				<annotation>
					<documentation>The name of a business account.</documentation>
				</annotation>
			</element>	
		</sequence>
		<attribute name="ownerID" type="string" use="required">
			<annotation>
				<documentation>The owner of business account.</documentation>
			</annotation>
		</attribute>
	</complexType>

	<!-- 
	Identifier types for order component
-->

	<complexType name="OrderIdentifierType">
		<annotation>
			<documentation>The type definition of an order identifier.</documentation>
		</annotation>
		<sequence>
			<element maxOccurs="1" minOccurs="0" name="UniqueID" type="string">
				<annotation>
					<documentation>The unique order ID.</documentation>
				</annotation>
			</element>
			<element maxOccurs="1" minOccurs="0" name="ExternalOrderID" type="string">
				<annotation>
					<documentation>The external order ID.</documentation>
				</annotation>
			</element>
			<element maxOccurs="1" minOccurs="0" name="CustomerOrderNumber" type="string">
				<annotation>
					<documentation>The customer order number.</documentation>
				</annotation>
			</element>			
		</sequence>
	</complexType>

	<complexType name="OrderItemIdentifierType">
		<annotation>
			<documentation>The type definition of an order item identifier.</documentation>
		</annotation>
		<sequence>
			<element maxOccurs="1" minOccurs="0" name="UniqueID" type="string">
				<annotation>
					<documentation>The order item ID.</documentation>
				</annotation>
			</element>
			<element maxOccurs="1" minOccurs="0" name="ExternalOrderItemID" type="string">
				<annotation>
					<documentation>The external order item ID.</documentation>
				</annotation>
			</element>
		</sequence>
	</complexType>

	<complexType name="FulfillmentCenterIdentifierType">
		<sequence>
			<element maxOccurs="1" minOccurs="0" name="UniqueID" type="string">
				<annotation>
					<documentation>
						The fulfillment center unique ID.
					</documentation>
				</annotation>
			</element>
			<element maxOccurs="1" minOccurs="0" name="Name" type="string">
				<annotation>
					<documentation>
						The fulfillment center name.
					</documentation>
				</annotation>
			</element>
		</sequence>
		<attribute name="ownerId" type="string" use="optional">
			<annotation>
				<documentation>The owner ID of the user that owns the fulfillment center.</documentation>
			</annotation>
		</attribute>
	</complexType>

	<complexType name="ShippingModeIdentifierType">
	   <sequence>
	   		<element maxOccurs="1" minOccurs="0" name="UniqueID" type="string">
				<annotation>
					<documentation>The shipping mode unique ID.</documentation>
				</annotation>
			</element>
			<element maxOccurs="1" minOccurs="0" name="ExternalIdentifier" type="_wcf:ExternalShipModeIdentifier">
				<annotation>
					<documentation>The external shipping mode identifier.</documentation>
				</annotation>
			</element>
		</sequence>
	</complexType>
	<complexType name="ExternalShipModeIdentifier">
		<sequence>
			<element maxOccurs="1" minOccurs="0" name="StoreUniqueID" type="string">
				<annotation>
					<documentation>The store ID with which the shipping mode is associated.</documentation>
				</annotation>
			</element>
			<element maxOccurs="1" minOccurs="0" name="ShipModeCode" type="string">
				<annotation>
					<documentation>The shipping mode code.</documentation>
				</annotation>
			</element>
			<element maxOccurs="1" minOccurs="0" name="Carrier" type="string">
				<annotation>
					<documentation>The shipping mode carrier.</documentation>
				</annotation>
			</element>		
		</sequence>
	</complexType>

	<!-- 
	Identifier types for foundation
-->
		
	<complexType name="StoreExternalIdentifierType">
		<annotation>
			<documentation>The type definition of a store external identifier.</documentation>
		</annotation>
		<sequence>
			<element maxOccurs="1" minOccurs="0" name="NameIdentifier" type="string">
				<annotation>
					<documentation>The identifier of the store.</documentation>
				</annotation>
			</element>
		</sequence>
		<attribute name="ownerID" type="string" use="optional">
			<annotation>
				<documentation>
					The owner organization of the store.
				</documentation>
			</annotation>
		</attribute>
		
	</complexType>

<!-- 
	Identifier types for store locator
-->	
	<complexType name="StoreIdentifierType">
		<annotation>
			<documentation>The type definition of a store identifier.</documentation>
		</annotation>
		<sequence>
			<element maxOccurs="1" minOccurs="0" name="UniqueID" type="string">
				<annotation>
					<documentation>The unique ID of the store.</documentation>
				</annotation>
			</element>
			<element maxOccurs="1" minOccurs="0" name="ExternalIdentifier" type="_wcf:StoreExternalIdentifierType">
				<annotation>
					<documentation>The external identifier of the store.</documentation>
				</annotation>
			</element>
			
		</sequence>
	</complexType>


	<complexType name="StoreLocationIdentifierType">
		<annotation>
			<documentation>The type definition of a store location identifier.</documentation>
		</annotation>
		<sequence>
			<element maxOccurs="1" minOccurs="0" name="UniqueID" type="string">
				<annotation>
					<documentation>The unique ID of the store location.</documentation>
