<schema xmlns="http://www.w3.org/2001/XMLSchema" xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore"
	ecore:package="com.ibm.commerce.inventory.datatypes" ecore:nsPrefix="Inventory"
	targetNamespace="http://www.ibm.com/xmlns/prod/commerce/inventory"
	xmlns:oa="http://www.openapplications.org/oagis/9"
	xmlns:wcf="http://www.ibm.com/xmlns/prod/commerce/foundation"
	xmlns:inv="http://www.ibm.com/xmlns/prod/commerce/inventory" elementFormDefault="qualified"
	attributeFormDefault="unqualified">

	<annotation>
		<documentation>
			This inventory XML schema file defines the ShowProductAvailability BOD which is used to
			show the availability of a product.
		</documentation>
	</annotation>

	<import namespace="http://www.openapplications.org/oagis/9"
		schemaLocation="../../../../../../Oagis9Dependencies.xsd" />

	<include schemaLocation="../Resources/Nouns/ProductAvailability.xsd" />

	<element name="ShowProductAvailability" type="inv:ShowProductAvailabilityType">
		<annotation>
			<documentation>
				Element definition of the ShowProductAvailability BOD.
			</documentation>
		</annotation>
	</element>

	<complexType name="ShowProductAvailabilityType">
		<annotation>
			<documentation>
				Type definition of the ShowProductAvailability BOD.
			</documentation>
		</annotation>
		<complexContent>
			<extension base="oa:BusinessObjectDocumentType">
				<sequence>
					<element name="DataArea" type="inv:ShowProductAvailabilityDataAreaType">
						<annotation>
							<documentation>
								Data area of the ShowProductAvailability BOD.
							</documentation>
						</annotation>
					</element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="ShowProductAvailabilityDataAreaType">
		<annotation>
			<documentation>Type definition of the ShowProductAvailability BOD's data area.</documentation>
		</annotation>
		<sequence>
			<element ref="oa:Show">
				<annotation>
					<documentation>The Show verb.</documentation>
				</annotation>
			</element>
			<element ref="inv:ProductAvailability" minOccurs="0" maxOccurs="unbounded">
				<annotation>
					<documentation>The ProductAvailability noun.</documentation>
				</annotation>
			</element>
		</sequence>
	</complexType>

</schema>
